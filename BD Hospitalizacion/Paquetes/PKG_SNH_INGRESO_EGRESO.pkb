 PROCEDURE PR_I_REL_DX_ING_EG (pRelDxIngEgId OUT SNH_REL_DIAGNOSTICOS_ING_EG.REL_DX_ING_EG_ID%TYPE,  
                               pIngresoId    IN SNH_MST_INGRESOS_EGRESOS.INGRESO_ID%TYPE,       
                               pTipoDxIngEg  IN CATALOGOS.SBC_CAT_CATALOGOS.CODIGO%TYPE, 
                               pDxId         IN HOSPITALARIO.SNH_MST_DIAGNOSTICOS.DIAGNOSTICO_ID%TYPE,
                               pTrasladoId   IN HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG.TRASLADO_ID%TYPE,
                               pUsuario      IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE,  
                               pResultado    OUT VARCHAR2,
                               pMsgError     OUT VARCHAR2) IS
 vFirma         MAXVARCHAR2 := kPAQUETE_FIRMA||'PR_I_REL_DX_ING_EG => ';                                
 vTipoDxIngEgId HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG.TIPO_DX_ING_EG_ID%TYPE;                      
 BEGIN
    vTipoDxIngEgId := HOSPITALARIO.PKG_SNH_UTILITARIOS.FN_OBT_CATALOGO_ID ('TPRLDX', pTipoDxIngEg);
    INSERT INTO SNH_REL_DIAGNOSTICOS_ING_EG (INGRESO_ID,
                                             DIAGNOSTICO_ID,
                                             TRASLADO_ID,
                                             TIPO_DX_ING_EG_ID,
                                             ESTADO_REGISTRO_ID,
                                             USUARIO_REGISTRO)
                                     VALUES (pIngresoId,
                                             pDxId,
                                             pTrasladoId,
                                             vTipoDxIngEgId,
                                             vGLOBAL_ESTADO_ACTIVO,
                                             pUsuario)
                                             RETURN REL_DX_ING_EG_ID INTO pRelDxIngEgId;
 EXCEPTION 
 WHEN OTHERS THEN
      pResultado := vFirma||'Se ha generado un error inesperado al tratar de crear la relación de diagnostico';
      pMsgError  := pResultado||' - '||SQLERRM;                                             
 END PR_I_REL_DX_ING_EG;  
 
 PROCEDURE PR_U_REL_DX_ING_EG (pRelDxIngEgId     IN SNH_REL_DIAGNOSTICOS_ING_EG.REL_DX_ING_EG_ID%TYPE,  
                               pIngresoId        IN SNH_MST_INGRESOS_EGRESOS.INGRESO_ID%TYPE,       
                               pTipoDxIngEg      IN CATALOGOS.SBC_CAT_CATALOGOS.CODIGO%TYPE, 
                               pDxId             IN HOSPITALARIO.SNH_MST_DIAGNOSTICOS.DIAGNOSTICO_ID%TYPE,
                               pTrasladoId       IN HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG.TRASLADO_ID%TYPE,
                               pEstadoRegistroId IN HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG.ESTADO_REGISTRO_ID%TYPE,
                               pUsuario          IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE,  
                               pResultado        OUT VARCHAR2,
                               pMsgError         OUT VARCHAR2) IS
 vFirma    MAXVARCHAR2 := kPAQUETE_FIRMA||'PR_U_REL_DX_ING_EG => ';
 vTipoDxIngEgId HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG.TIPO_DX_ING_EG_ID%TYPE; 
 BEGIN
     dbms_output.put_line ('Dentro de update dx');
     dbms_output.put_line ('pRelDxIngEgId: '||pRelDxIngEgId);
     dbms_output.put_line ('pIngresoId: '||pIngresoId);
     dbms_output.put_line ('pTipoDxIngEg: '||pTipoDxIngEg);
     dbms_output.put_line ('pDxId: '||pDxId);
     dbms_output.put_line ('pTrasladoId: '||pTrasladoId); 
     dbms_output.put_line ('pEstadoRegistroId: '||pEstadoRegistroId);    
     CASE
     WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ELIMINADO THEN
         <<EliminaRegistro>>
          BEGIN
             UPDATE HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG
                SET ESTADO_REGISTRO_ID   = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,
                    USUARIO_ELIMINA      = pUsuario,
                    FECHA_ELIMINA        = CURRENT_TIMESTAMP
              WHERE REL_DX_ING_EG_ID = pRelDxIngEgId AND
                    ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
          EXCEPTION
             WHEN OTHERS THEN
                  pResultado := 'Error no controlado al eliminar registro [Id] - '||pDxId;
                  pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                  RETURN;                
          END EliminaRegistro;
          pResultado := 'Se ha eliminado el registro. [Id:'||pDxId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_PASIVO THEN
         <<PasivaRegistro>>       
         BEGIN
             UPDATE HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG
                SET ESTADO_REGISTRO_ID   = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,  
                    USUARIO_PASIVA       = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                           ELSE USUARIO_PASIVA
                                           END,    
                    FECHA_PASIVA         = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                           ELSE FECHA_PASIVA
                                           END
             WHERE REL_DX_ING_EG_ID = pRelDxIngEgId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;    
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al pasivar registro [Id] - '||pDxId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END PasivaRegistro;
         pResultado := 'Se ha pasivado el registro. [Id:'||pDxId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN
         <<ActivarRegistro>>
         BEGIN
            UPDATE HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG
               SET ESTADO_REGISTRO_ID   = pEstadoRegistroId, 
                   USUARIO_MODIFICACION = pUsuario,    
                   USUARIO_PASIVA       = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                          ELSE USUARIO_PASIVA
                                          END,    
                   FECHA_PASIVA         = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                          ELSE FECHA_PASIVA
                                          END
             WHERE REL_DX_ING_EG_ID = pRelDxIngEgId AND
