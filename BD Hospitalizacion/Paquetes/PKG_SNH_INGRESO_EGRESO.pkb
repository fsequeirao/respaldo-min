            vTipoDxIngEgId := HOSPITALARIO.PKG_SNH_UTILITARIOS.FN_OBT_CATALOGO_ID ('TPRLDX', pTipoDxIngEg);  
            UPDATE HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG
               SET INGRESO_ID        = pIngresoId,  
                   DIAGNOSTICO_ID    = pDxId, 
                   TRASLADO_ID       = pTrasladoId,       
                   TIPO_DX_ING_EG_ID = vTipoDxIngEgId,    
