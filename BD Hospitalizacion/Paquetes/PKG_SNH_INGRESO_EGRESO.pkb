                  RELUSALSERV.FECHA_REGISTRO             ADSERV_RELUSALSERV_FEC_REG, 
                  CFG.CODIGO_ASISTENCIAL                 CFG_COD_ASISTENCIAL,       
                  CFG.SALA_ID                            CFG_SALA_ID,                  
                  CFG.HABITACION_ID                      CFG_HABITACION_ID,            
