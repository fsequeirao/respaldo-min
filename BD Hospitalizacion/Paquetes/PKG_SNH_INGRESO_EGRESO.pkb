 vRegistros var_refcursor;
 BEGIN
 OPEN vRegistros FOR
 SELECT JSON_SERIALIZE (
                       JSON_ARRAYAGG (
                                JSON_OBJECT (
                                             KEY 'id' VALUE ING.INGRESO_ID,
                                             KEY 'admisionServicio' VALUE (
                                                                           JSON_OBJECT ( 
                                                                                        KEY 'id' VALUE ING.ADMISION_SERVICIO_ID
                                                                                       )
                                                                           ),
                                             KEY 'diagnosticos' VALUE (
                                                             SELECT JSON_SERIALIZE (
                                                                              JSON_ARRAYAGG (
                                                                                                JSON_OBJECT (
                                                                                                             KEY 'id' VALUE RELDX.DIAGNOSTICO_ID,
                                                                                                             KEY 'mperSalud' VALUE (
                                                                                                                                    JSON_OBJECT (
                                                                                                                                                KEY 'id' VALUE DX.MPERS_SALUD_ID
                                                                                                                                                )
                                                                                                                                    ),
                                                                                                             KEY 'tipoDiagnostico' VALUE (
                                                                                                                              JSON_OBJECT (
                                                                                                                                          KEY 'id' VALUE CATTIPDX.CATALOGO_ID,
                                                                                                                                          KEY 'codigo' VALUE CATTIPDX.CODIGO,
                                                                                                                                          KEY 'valor' VALUE CATTIPDX.VALOR,
                                                                                                                                          KEY 'nombre' VALUE CATTIPDX.DESCRIPCION
                                                                                                                                          )
                                                                                                                             ),
                                                                                                             KEY 'diagnostico' VALUE DX.DIAGNOSTICO,
                                                                                                             KEY 'usuarioCodifica' VALUE DX.USR_CODIFICA_CIE,
                                                                                                             KEY 'cie' VALUE (
                                                                                                                              JSON_OBJECT (
                                                                                                                                           KEY 'id' VALUE DX.CIE_ID,
                                                                                                                                           KEY 'codigo' VALUE CIE.CLAVE,
                                                                                                                                           KEY 'nombre' VALUE CIE.NOMBRE
                                                                                                                                          )
                                                                                                                             ),
                                                                                                             KEY 'relDiagnostico' VALUE (
                                                                                                                                        JSON_OBJECT (
                                                                                                                                                    KEY 'id' VALUE RELDX.REL_DX_ING_EG_ID,
                                                                                                                                                    KEY 'tipoDxIngEgreso' VALUE (
                                                                                                                                                                                JSON_OBJECT (
                                                                                                                                                                                            KEY 'id' VALUE CATTIPDXING.CATALOGO_ID,
                                                                                                                                                                                            KEY 'codigo' VALUE CATTIPDXING.CODIGO,
                                                                                                                                                                                            KEY 'valor' VALUE CATTIPDXING.VALOR,
                                                                                                                                                                                            KEY 'nombre' VALUE CATTIPDXING.DESCRIPCION
                                                                                                                                                                                            )
                                                                                                                                                                                )
                                                                                                                                                    )
                                                                                                                                        ) ,
                                                                                                             KEY 'estadoRegistro' VALUE (
                                                                                                                                         JSON_OBJECT ( KEY 'id' VALUE ESTREGCAT.CATALOGO_ID,
                                                                                                                                                       KEY 'codigo' VALUE ESTREGCAT.CODIGO,
                                                                                                                                                       KEY 'valor' VALUE ESTREGCAT.VALOR,
                                                                                                                                                       KEY 'nombre' VALUE ESTREGCAT.DESCRIPCION
                                                                                                                                                      )
                                                                                                                                         )/*, 
                                                                                                             KEY 'controlRegistro' VALUE (
                                                                                                                                          JSON_OBJECT ( 
                                                                                                                                                       KEY 'registro' VALUE (
                                                                                                                                                                            JSON_OBJECT (
                                                                                                                                                                                         KEY 'usuario' VALUE (
                                                                                                                                                                                                             JSON_OBJECT (
                                                                                                                                                                                                                          KEY 'id'     VALUE NULL, 
                                                                                                                                                                                                                          KEY 'nombre' VALUE DX.USUARIO_REGISTRO
                                                                                                                                                                                                                          )
                                                                                                                                                                                                              ),
                                                                                                                                                                                          KEY 'fecha' VALUE DX.FECHA_REGISTRO 
                                                                                                                                                                                         )
                                                                                                                                                                             ),
                                                                                                                                                       KEY 'modificacion' VALUE (
                                                                                                                                                                                JSON_OBJECT (
                                                                                                                                                                                             KEY 'usuario' VALUE (
                                                                                                                                                                                                                  JSON_OBJECT (
                                                                                                                                                                                                                               KEY 'id'     VALUE NULL, 
                                                                                                                                                                                                                               KEY 'nombre' VALUE DX.USUARIO_MODIFICACION
                                                                                                                                                                                                                               )
                                                                                                                                                                                                                  ),
                                                                                                                                                                                             KEY 'fecha' VALUE DX.FECHA_MODIFICACION 
                                                                                                                                                                                              )
                                                                                                                                                                                  ) --,    
--                                                                                                                                                       KEY 'pasivado' VALUE (
--                                                                                                                                                                              JSON_OBJECT (
--                                                                                                                                                                                           KEY 'usuario' VALUE (
--                                                                                                                                                                                                                JSON_OBJECT (
--                                                                                                                                                                                                                             KEY 'id'     VALUE NULL, 
--                                                                                                                                                                                                                             KEY 'nombre' VALUE DX.USUARIO_PASIVA
--                                                                                                                                                                                                                             )
--                                                                                                                                                                                                                ),
--                                                                                                                                                                                           KEY 'fecha' VALUE DX.FECHA_PASIVA
--                                                                                                                                                                                           )
--                                                                                                                                                                              ) ,    
--                                                                                                                                                       KEY 'eliminado' VALUE (
--                                                                                                                                                                               JSON_OBJECT (
--                                                                                                                                                                                            KEY 'usuario' VALUE (
--                                                                                                                                                                                                                JSON_OBJECT (
--                                                                                                                                                                                                                             KEY 'id'     VALUE NULL, 
--                                                                                                                                                                                                                             KEY 'nombre' VALUE DX.USUARIO_ELIMINA
--                                                                                                                                                                                                                             )
--                                                                                                                                                                                                                 ),
--                                                                                                                                                                                            KEY 'fecha' VALUE DX.FECHA_ELIMINA
--                                                                                                                                                                                            )
--                                                                                                                                                                              )                                                                                                                                                                                                                                                                                                                                                                                        
                                                                                                                                                       )
                                                                                                                                          )*/       
                                                                                                            ) 
                                                                                             RETURNING  CLOB
                                                                                             ) 
                                                                                      RETURNING  CLOB ---NULL ON EMPTY   
                                                                                     ) json 
                                                                               FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG RELDX
                                                                               JOIN HOSPITALARIO.SNH_MST_DIAGNOSTICOS DX
                                                                                 ON DX.DIAGNOSTICO_ID = RELDX.DIAGNOSTICO_ID AND
                                                                                    DX.TIPO_DIAGNOSTICO = pTipoDxId AND
                                                                                    DX.ADMISION_SERVICIO_ID = ING.ADMISION_SERVICIO_ID
                                                                               JOIN CATALOGOS.SBC_CAT_CATALOGOS CATTIPDX
                                                                                 ON CATTIPDX.CATALOGO_ID = DX.TIPO_DIAGNOSTICO
                                                                               JOIN CATALOGOS.SBC_CAT_CATALOGOS CATTIPDXING
                                                                                 ON CATTIPDXING.CATALOGO_ID = RELDX.TIPO_DX_ING_EG_ID  
                                                                               JOIN CATALOGOS.SBC_CAT_CATALOGOS ESTREGCAT
                                                                                 ON ESTREGCAT.CATALOGO_ID = DX.ESTADO_REGISTRO  
                                                                              LEFT JOIN HOSPITALARIO.SNH_CIE10 CIE
                                                                                ON CIE.CIE10_ID = DX.CIE_ID           
                                                                               WHERE RELDX.INGRESO_ID = ING.INGRESO_ID
                                                                       ),
--                                             
                                             KEY 'controlRegistro' VALUE (
                                                                          JSON_OBJECT ( 
                                                                                       KEY 'registro' VALUE (
                                                                                                            JSON_OBJECT (
                                                                                                                         KEY 'usuario' VALUE (
                                                                                                                                             JSON_OBJECT (
                                                                                                                                                          KEY 'id'     VALUE NULL, 
                                                                                                                                                          KEY 'nombre' VALUE ING.USUARIO_REGISTRO
                                                                                                                                                          )
                                                                                                                                              ),
                                                                                                                          KEY 'fecha' VALUE ING.FECHA_REGISTRO 
                                                                                                                         )
                                                                                                             ),
                                                                                       KEY 'modificacion' VALUE (
                                                                                                                JSON_OBJECT (
                                                                                                                             KEY 'usuario' VALUE (
                                                                                                                                                  JSON_OBJECT (
                                                                                                                                                               KEY 'id'     VALUE NULL, 
                                                                                                                                                               KEY 'nombre' VALUE ING.USUARIO_MODIFICACION
                                                                                                                                                               )
                                                                                                                                                  ),
                                                                                                                             KEY 'fecha' VALUE ING.FECHA_MODIFICACION 
                                                                                                                              )
                                                                                                                  ) ,    
                                                                                       KEY 'pasivado' VALUE (
                                                                                                              JSON_OBJECT (
                                                                                                                           KEY 'usuario' VALUE (
                                                                                                                                                JSON_OBJECT (
                                                                                                                                                             KEY 'id'     VALUE NULL, 
                                                                                                                                                             KEY 'nombre' VALUE ING.USUARIO_PASIVA
                                                                                                                                                             )
                                                                                                                                                ),
                                                                                                                           KEY 'fecha' VALUE ING.FECHA_PASIVA
                                                                                                                           )
                                                                                                              ) ,    
                                                                                       KEY 'eliminado' VALUE (
                                                                                                               JSON_OBJECT (
                                                                                                                            KEY 'usuario' VALUE (
                                                                                                                                                JSON_OBJECT (
                                                                                                                                                             KEY 'id'     VALUE NULL, 
                                                                                                                                                             KEY 'nombre' VALUE ING.USUARIO_ELIMINA
                                                                                                                                                             )
                                                                                                                                                 ),
                                                                                                                            KEY 'fecha' VALUE ING.FECHA_ELIMINA
                                                                                                                            )
                                                                                                              )                                                                                                                                                                                                                                                                                                                                                                                        
                                                                                       )
                                                                          )                                             
--                                             
                                             )
                                       RETURNING CLOB
                                      ) 
                       RETURNING CLOB NULL ON EMPTY
                       )diagnosticos
FROM HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS ING
WHERE ING.ADMISION_SERVICIO_ID = pAdminServId AND
      EXISTS (SELECT 1
              FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG A
             WHERE A.INGRESO_ID = ING.INGRESO_ID);
 
 RETURN vRegistros; 
 END FN_OBT_DX_ADMINSERV_TIPO_DX_ID; 
 
 FUNCTION FN_OBT_DATOS_DIAGNOSTICOS(pConsulta IN HOSPITALARIO.OBJ_DIAGNOSTICOS,
                                    pFuente   IN NUMBER) RETURN var_refcursor AS
 vRegistros var_refcursor;
 BEGIN
 CASE pFuente
 WHEN 1 THEN
     vRegistros := FN_OBT_DX_INGR_ADMISERV_ID (pConsulta.IngresoId, pConsulta.AdminServId);
 WHEN 3 THEN
     vRegistros := FN_OBT_DX_ADMSERV_MPERSALUD_ID (pConsulta.AdminServId, pConsulta.mperSaludId);
 WHEN 4 THEN
     vRegistros := FN_OBT_DX_INGR_MPERSALUD_ID (pConsulta.IngresoId, pConsulta.mperSaludId); 
 WHEN 5 THEN
     vRegistros := FN_OBT_DX_INGR_TIPDXINGEG (pConsulta.IngresoId, pConsulta.TipoDxIngEg);
 WHEN 6 THEN
     vRegistros := FN_OBT_DX_ADMSERV_TIPDXINGEG (pConsulta.AdminServId, pConsulta.TipoDxIngEg);   
 WHEN 7 THEN
     vRegistros := FN_OBT_DX_ID (pConsulta.diagnosticoId);       
 WHEN 8 THEN
     vRegistros := FN_OBT_DX_RELINGEGR_ID (pConsulta.RelDxIngEgId);     
 WHEN 9 THEN
      vRegistros := FN_OBT_DX_INGRESOID (pConsulta.IngresoId);
 WHEN 11 THEN
      vRegistros := FN_OBT_DX_ADMIS_SERV_ID (pConsulta.AdminServId); 
 WHEN 12 THEN
      vRegistros := FN_OBT_DX_MPERSALUD_ID (pConsulta.mperSaludId);    
 WHEN 13 THEN
      vRegistros := FN_OBT_DX_INGRESO_TIPO_DX_ID (pConsulta.IngresoId, pConsulta.TipoDxId);    
 WHEN 14 THEN
      vRegistros := FN_OBT_DX_ADMINSERV_TIPO_DX_ID (pConsulta.AdminServId, pConsulta.TipoDxId);                
 END CASE;
 RETURN vRegistros;
 END FN_OBT_DATOS_DIAGNOSTICOS;
  
 PROCEDURE PR_C_DIAGNOSTICOS (pConsulta        IN HOSPITALARIO.OBJ_DIAGNOSTICOS,
                              pPgn             IN NUMBER,            
                              pPgnAct          IN NUMBER default 1,  
                              pPgnTmn          IN NUMBER default 100,
                              pDatosPaginacion OUT var_refcursor,    
                              pRegistro        OUT var_refcursor, 
                              pResultado       OUT VARCHAR2,     
                              pMsgError        OUT VARCHAR2)IS
 vFirma         MAXVARCHAR2 := kPAQUETE_FIRMA||'PR_C_MST_NOTAS => ';
 vCantRegistros SIMPLE_INTEGER := 0;
 vPaginacion    HOSPITALARIO.PKG_CATALOGOS_UTIL.assoc_array_str_datos; 
 vFuente        SIMPLE_INTEGER := 0;
 vPgn           BOOLEAN := TRUE;
 BEGIN
     CASE
     WHEN (FN_VAL_EXISTE_DIAGNOSTICO (pConsulta, vPgn, 
                                      vCantRegistros, vFuente)) = TRUE THEN 
                  DBMS_OUTPUT.PUT_LINE ('Existe mst notas: '||pMsgError);   
                   HOSPITALARIO.PKG_CATALOGOS_UTIL.PR_INDC_PAGINACION_PERSONA(PREGISTROS  => vCantRegistros, 
                                                                              pPgnAct     => pPgnAct, 
                                                                              pPgnTmn     => pPgnTmn, 
                                                                              pPaginacion => vPaginacion, 
                                                                              pMsg        => pMsgError);
                 DBMS_OUTPUT.PUT_LINE ('sale de paginación: '||pMsgError);                                                          
                 CASE 
                 WHEN pMsgError IS NOT NULL THEN 
                      pResultado := pMsgError;
                      pMsgError  := pMsgError;
                      RAISE eSalidaConError;
                 ELSE 
                      pDatosPaginacion := FN_OBT_DATOS_PAGINACION (pDatosPaginacion =>  vPaginacion);
                 END CASE;            
                 PR_I_TABLA_TEMP_DIAGNOSTICOS (pConsulta       => pConsulta,
                                               pPgnAct         => pPgnAct,        
                                               pPgnTmn         => pPgnTmn,       
                                               pTipoPaginacion => vFuente,
                                               pResultado      => pResultado,     
                                               pMsgError       => pMsgError); 
                                 dbms_output.put_line ('Error saliendo de I tabla temporal PR_I_TABLA_TEMP_DIAGNOSTICOS: '||pMsgError);              
                                 CASE
                                 WHEN pMsgError IS NOT NULL THEN 
                                      RAISE eSalidaConError;
                                 ELSE NULL;
                                 END CASE;
           pRegistro := FN_OBT_DATOS_DIAGNOSTICOS(pConsulta, vFuente);
     ELSE
           pResultado := 'No se encontraron registros con los parámetros enviados';
           RAISE eRegistroNoExiste;
     END CASE;
 EXCEPTION
 WHEN eSalidaConError THEN
      pResultado := pResultado;
      pMsgError  := vFirma||pResultado;  
 WHEN eRegistroNoExiste THEN
      pResultado := pResultado;
      pMsgError  := vFirma||pResultado;
 WHEN OTHERS THEN
      pResultado := 'Error no controlado al querer obtener información de diagnosticos.';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM;
 END PR_C_DIAGNOSTICOS;
 
 
 PROCEDURE PR_CRUD_DIAGNOSTICO (pIngresoId       IN HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.INGRESO_ID%TYPE,
                                pAdminServId     IN HOSPITALARIO.SNH_REL_ADMSRV_CAMAS.ADMISION_SERVICIO_ID%TYPE,
                                pTipoDiagnostico IN CATALOGOS.SBC_CAT_CATALOGOS.CODIGO%TYPE,
                                pUsuario         IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE,
                                pAccionEstado    IN VARCHAR2,                                
                                pFecInicio       IN DATE,                                    
                                pFecFin          IN DATE,                                    
                                pJson            IN VARCHAR2,          
                                pTipoAccion      IN VARCHAR2,          
                                pPgn             IN NUMBER,            
                                pPgnAct          IN NUMBER default 1,  
                                pPgnTmn          IN NUMBER default 100,
                                pDatosPaginacion OUT var_refcursor,    
                                pRegistro        OUT var_refcursor,    
                                pResultado       OUT VARCHAR2,         
                                pMsgError        OUT VARCHAR2)IS
                                
 vFirma             MAXVARCHAR2 := kPAQUETE_FIRMA||'PR_CRUD_DIAGNOSTICO => ';
 vResultado         MAXVARCHAR2;
 vEstadoRegistroId  CATALOGOS.SBC_CAT_CATALOGOS.CATALOGO_ID%TYPE;  
 vIdJson            HOSPITALARIO.SNH_JSON_DATA.ID%TYPE;
 
 vRegDxRelIngEg     refDxRelIngEg;
 vVarRegDxRelIngEg  DIAGNOSTICOS_REL_ING_EGR;

 vExisteDetalle     SIMPLE_INTEGER := 0;
 
 vDxId              HOSPITALARIO.SNH_MST_DIAGNOSTICOS.DIAGNOSTICO_ID%TYPE;  

 vRelDxIngEgId      SNH_REL_DIAGNOSTICOS_ING_EG.REL_DX_ING_EG_ID%TYPE;   
 
 vJson              BOOLEAN := FALSE;                     
 BEGIN
      CASE
      WHEN pTipoAccion IS NULL THEN 
           pResultado := 'El párametro pTipoAccion no puede venir NULL';
           pMsgError  := pResultado;
           RAISE eParametroNull;
      ELSE NULL;
      END CASE;
      CASE
      WHEN pTipoAccion = kINSERT THEN
           CASE
           WHEN pUsuario IS NULL THEN
                pResultado := 'El usuario no puede venir nulo.';
                pMsgError  := pResultado;
                RAISE eParametroNull;
           ELSE 
                CASE -- validamos que el usuario se valido, con mst_usuarios
                WHEN (PKG_SNH_UTILITARIOS.FN_VALIDAR_USUARIO (pUsuario)) = FALSE THEN
                      pResultado := 'Usuario no valido';
                      pMsgError  := pResultado;
                      RAISE eRegistroNoExiste;
                ELSE NULL;
                END CASE;
           END CASE;
           dbms_output.put_line ('Sale de validar usuario');
           CASE
           WHEN pJson IS NOT NULL THEN
                dbms_output.put_line ('Trae parámetro json');
                PR_I_JSON_DATA (pIdJson    => vIdJson,
                                pJson      => pJson,
                                pResultado => pResultado,           
                                pMsgError  => pMsgError); 
                    dbms_output.put_line ('vIdJson: '||vIdJson); 
                    dbms_output.put_line ('Saliendo de json pMsgError: '||pMsgError);             
                    CASE
                    WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                         RAISE eSalidaConError;
                    ELSE 
                        PR_C_JSON_DATA_DIAGNOSTICOS (pIdJson    => vIdJson,
                                                     pRegistro  => vRegDxRelIngEg,  --pRegistro,
                                                     pResultado => pResultado,
                                                     pMsgError  => pMsgError);
                                 dbms_output.put_line ('Saliendo de PR_C_JSON_DATA_NOTAS pMsgError: '||pMsgError);                 
                                CASE
                                WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                                     RAISE eSalidaConError;
                                ELSE 
                                     dbms_output.put_line ('antes de asignar variables de cursor');
                                     LOOP
                                     FETCH vRegDxRelIngEg INTO vVarRegDxRelIngEg;
                                     vExisteDetalle := vRegDxRelIngEg%ROWCOUNT;
                                     EXIT WHEN vRegDxRelIngEg%NOTFOUND; 
                                          PR_I_DIAGNOSTICOS (pDxId                 => vDxId,
                                                             pAdminServId          => vVarRegDxRelIngEg.admisionServId,
                                                             pMpersSaludId         => vVarRegDxRelIngEg.mpersSaludId,
                                                             pCieId                => vVarRegDxRelIngEg.cieId,
                                                             pMperSaludCieCodifica => vVarRegDxRelIngEg.usrCodificaCie,
                                                             pTipDxCod             => vVarRegDxRelIngEg.TipoDiagnostico,
                                                             pDiagnostico          => vVarRegDxRelIngEg.diagnostico,
                                                             pUsuario              => pUsuario,
                                                             pResultado            => pResultado,
                                                             pMsgError             => pMsgError);
                                          CASE
                                          WHEN pMsgError IS NOT NULL THEN
                                               RAISE eSalidaConError;
                                          ELSE
                                              PR_CRUD_REL_DX_ING_EG (pRelDxIngEgId    => vRelDxIngEgId, 
                                                                     pIngresoId       => vVarRegDxRelIngEg.ingresoId, 
                                                                     pTipoDxIngEg     => vVarRegDxRelIngEg.tipoDxCodigo,  
                                                                     pDxId            => vDxId,  
                                                                     pTrasladoId      => NULL, 
                                                                     pUsuario         => pUsuario,  
                                                                     pAccionEstado    => NULL, 
                                                                     pFecInicio       => NULL, 
                                                                     pFecFin          => NULL, 
                                                                     pJson            => NULL, 
                                                                     pTipoAccion      => kINSERT, 
                                                                     pPgn             => NULL,  
                                                                     pDatosPaginacion => pDatosPaginacion, 
                                                                     pRegistro        => pRegistro,   
                                                                     pResultado       => pResultado, 
                                                                     pMsgError        => pMsgError); 
                                              CASE
                                              WHEN pMsgError IS NOT NULL THEN
                                                  RAISE eSalidaConError;
                                              ELSE NULL;
                                              END CASE;                           
                                          END CASE;
                                     END LOOP;        
                                     CLOSE vRegDxRelIngEg;
                                     dbms_output.put_line ('despues de asignar variables de cursor');
                                     
                                     PR_C_DIAGNOSTICOS (pConsulta       => HOSPITALARIO.OBJ_DIAGNOSTICOS (
                                                                                                         diagnosticoId => null,
                                                                                                         RelDxIngEgId  => null,
                                                                                                         IngresoId     => vVarRegDxRelIngEg.ingresoId,
                                                                                                         TrasladoId    => null,
                                                                                                         AdminServId   => null,
                                                                                                         mperSaludId   => null,
                                                                                                         fechaSolucion => null,
                                                                                                         TipoDxId      => null,
                                                                                                         TipoDxIngEg   => null,
                                                                                                         FecInicio     => null,
                                                                                                         FecFin        => null
                                                                                                         ),
                                                        pPgn             => pPgn,            
                                                        pPgnAct          => pPgnAct,          
                                                        pPgnTmn          => pPgnTmn,         
                                                        pDatosPaginacion => pDatosPaginacion,
                                                        pRegistro        => pRegistro,       
                                                        pResultado       => pResultado,
                                                        pMsgError        => pMsgError);
                                     CASE
                                     WHEN pMsgError IS NOT NULL THEN
                                         RAISE eSalidaConError;
                                     ELSE vResultado := 'Se crean exitosamente registros de diagnosticos para el ingreso id: '||vVarRegDxRelIngEg.ingresoId||', devolviendo el JSon de este';
                                     END CASE;                                                         
                                END CASE;                        
                    END CASE;
                    vIdJson := null;                                 
           ELSE NULL;
           END CASE;
      WHEN pTipoAccion = kUPDATE THEN
           CASE
           WHEN pUsuario IS NULL THEN
                pResultado := 'El usuario no puede venir nulo.';
                pMsgError  := pResultado;
                RAISE eParametroNull;
           ELSE 
                CASE -- validamos que el usuario se valido, con mst_usuarios
                WHEN (PKG_SNH_UTILITARIOS.FN_VALIDAR_USUARIO (pUsuario)) = FALSE THEN
                      pResultado := 'Usuario no valido';
                      pMsgError  := pResultado;
                      RAISE eRegistroNoExiste;
                ELSE NULL;
                END CASE;
           END CASE;      
                      
           CASE
           WHEN pAccionEstado = 0 THEN
                vEstadoRegistroId := vGLOBAL_ESTADO_ACTIVO;
           WHEN pAccionEstado = 1 THEN
                vEstadoRegistroId := vGLOBAL_ESTADO_PASIVO;
           ELSE NULL;
           END CASE;
---- fsequiera
           CASE
           WHEN pJson IS NOT NULL THEN
                dbms_output.put_line ('Trae parámetro json detalle notas');
                PR_I_JSON_DATA (pIdJson    => vIdJson,
                                pJson      => pJson,
                                pResultado => pResultado,           
                                pMsgError  => pMsgError); 
                    dbms_output.put_line ('vIdJson det notas: '||vIdJson); 
                    dbms_output.put_line ('Saliendo de json det notas pMsgError: '||pMsgError);             
                    CASE
                    WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                         RAISE eSalidaConError;
                    ELSE 
                        PR_C_JSON_DATA_DIAGNOSTICOS (pIdJson    => vIdJson,
                                                     pRegistro  => vRegDxRelIngEg,  --pRegistro,
                                                     pResultado => pResultado,
                                                     pMsgError  => pMsgError);
                                dbms_output.put_line ('Saliendo de PR_C_JSON_DATA_DIAGNOSTICOS pMsgError: '||pMsgError);                
                                CASE
                                WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                                     RAISE eSalidaConError;
                                ELSE NULL;
                                END CASE;                        
                    END CASE;
           vJson := TRUE;                                          
           ELSE NULL;
           END CASE;
           CASE
           WHEN vJson THEN
