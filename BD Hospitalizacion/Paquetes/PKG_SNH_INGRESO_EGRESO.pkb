                  CATESREG.CODIGO                        CATESREG_CODIGO,
                  CATESREG.VALOR                         CATESREG_VALOR,
                  CATESREG.DESCRIPCION                   CATESREG_DESC,
                  CFG.USUARIO_REGISTRO                   CFG_USR_REGISTRO,           
