                    ELSE 
                        PR_C_JSON_DATA_DIAGNOSTICOS (pIdJson    => vIdJson,
                                                     pRegistro  => vRegDxRelIngEg,  --pRegistro,
                                                     pResultado => pResultado,
                                                     pMsgError  => pMsgError);
                                dbms_output.put_line ('Saliendo de PR_C_JSON_DATA_DIAGNOSTICOS pMsgError: '||pMsgError);                
                                CASE
                                WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                                     RAISE eSalidaConError;
                                ELSE NULL;
                                END CASE;                        
                    END CASE;
           vJson := TRUE;                                          
           ELSE NULL;
           END CASE;
           CASE
           WHEN vJson THEN
                  dbms_output.put_line ('antes de asignar variables de cursor detalle');
                 LOOP
                 FETCH vRegDxRelIngEg 
                  INTO vVarRegDxRelIngEg;
