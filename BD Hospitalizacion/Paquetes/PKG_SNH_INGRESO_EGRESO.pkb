 PROCEDURE PR_C_JSON_DATA_DIAGNOSTICOS (pIdJson    IN HOSPITALARIO.SNH_JSON_DATA.ID%TYPE,
                                        pRegistro  OUT refDxRelIngEg,
                                        pResultado OUT VARCHAR2,     
                                        pMsgError  OUT VARCHAR2) IS
 --vRegistro var_refcursor;
 vFirma MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_C_JSON_DATA_DIAGNOSTICOS => ';
 BEGIN
    DBMS_OUTPUT.PUT_LINE ('Dentro del begin de c_json: '||pIdJson);
    OPEN pRegistro FOR
        WITH jdata AS (
         SELECT json_serialize(a.info PRETTY) info
           FROM hospitalario.SNH_JSON_DATA a
          WHERE ID = pIdJson
            )
            SELECT t.*
            FROM jdata, json_table (
                INFO, '$'
                COLUMNS (
                  IngresoId      number   PATH '$.id',
                  AdmisionServId number path '$.admisionServicio.id',
                  NESTED PATH '$.diagnosticos[*]'
                  COLUMNS (
                     DiagnosticoId         number   PATH '$.id',
                     mpersaludId           number   PATH '$.mperSalud.id',
                     cieId                 number   PATH '$.cie.id',
                     usrCodifica           number   PATH '$.usuarioCodifica.id',
                     RelDiagnosticoId      number   PATH '$.relDiagnostico.id',
                 --    TipoDxIngEgresoId     number   PATH '$.RelDiagnostico.tipoDxIngEgreso.id',
                     TipoDxIngEgresoCodigo varchar2 PATH '$.relDiagnostico.tipoDxIngEgreso.codigo',
                     tipoDxCodigo          varchar2 PATH '$.tipoDiagnostico.codigo',
                     DxDescripcion         varchar2 PATH '$.diagnostico'
                  )
                )
              ) t;
 EXCEPTION
 WHEN OTHERS THEN
      pResultado := 'Error no controlado al insertar el parámetro json';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM;         
 END PR_C_JSON_DATA_DIAGNOSTICOS; 
 
 
