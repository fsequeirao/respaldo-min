 PROCEDURE PR_C_JSON_DATA_DETNOTAS (pIdJson    IN HOSPITALARIO.SNH_JSON_DATA.ID%TYPE,
                                    pRegistro  OUT refDetNotas,
                                    pResultado OUT VARCHAR2,     
                                    pMsgError  OUT VARCHAR2) IS
 --vRegistro var_refcursor;
 vFirma MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_C_JSON_DATA_DETNOTAS => ';
 vVarRegDetNotas    detNotas;
 BEGIN
    DBMS_OUTPUT.PUT_LINE ('Dentro del begin de c_json detalle: '||pIdJson);
    OPEN pRegistro FOR
         WITH jdata AS (
         SELECT json_serialize(a.info PRETTY) info
           FROM hospitalario.SNH_JSON_DATA a
          WHERE ID = pIdJson
            )
            SELECT t.*
            FROM jdata, json_table (
                INFO, '$'
                COLUMNS (
                  idMstNotas number path '$.id',
                  NESTED  PATH '$.detalleNotas[*]' 
                  COLUMNS (
                    idDetNota       number   PATH '$.id',
                    descripcionNota varchar2 PATH '$.descripcionNota',
                    comentarios     varchar2 PATH '$.comentarios',
                    NESTED  PATH '$.descripcionNota[*]'    --NESTED  PATH '$.tipoDetNota[*]' 
                                              COLUMNS (
                                                       idTipoDetNota number PATH '$.id',
                                                       codigo        varchar2 PATH '$.codigo'
                                                       )        
                  )
                )
              ) t;
              
 EXCEPTION
 WHEN OTHERS THEN
      pResultado := 'Error no controlado al insertar el parámetro json';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM;         
 END PR_C_JSON_DATA_DETNOTAS;
 
 PROCEDURE PR_I_DET_NOTAS (pPerSaludEvoNotaId OUT HOSPITALARIO.SNH_DET_NOTAS.PERSALUD_EVO_NOTA_ID%TYPE,
                           pDetNotaId         IN HOSPITALARIO.SNH_DET_NOTAS.DET_NOTA_ID%TYPE,
                           pTipNotaDetId      IN HOSPITALARIO.SNH_DET_NOTAS.TIPO_DET_NOTA_ID%TYPE,
                           pComentario        IN HOSPITALARIO.SNH_DET_NOTAS.COMENTARIOS%TYPE,
                           pUsuario           IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE, 
                           pResultado         OUT VARCHAR2,     
                           pMsgError          OUT VARCHAR2) IS
 vFirma MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_I_DET_NOTAS => ';
 BEGIN
     INSERT INTO HOSPITALARIO.SNH_DET_NOTAS (DET_NOTA_ID,
                                             TIPO_DET_NOTA_ID,
                                             COMENTARIOS,
                                             ESTADO_REGISTRO_ID,
                                             USUARIO_REGISTRO)
                                     VALUES (pDetNotaId,
                                             pTipNotaDetId,
                                             pComentario,
                                             vGLOBAL_ESTADO_ACTIVO,
                                             pUsuario)
                                             RETURNING PERSALUD_EVO_NOTA_ID INTO pPerSaludEvoNotaId;
 EXCEPTION
 WHEN OTHERS THEN
      pResultado := 'Error no controlado';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM; 
 END PR_I_DET_NOTAS;
 
 PROCEDURE PR_U_DET_NOTAS (pPerSaludEvoNotaId IN HOSPITALARIO.SNH_DET_NOTAS.PERSALUD_EVO_NOTA_ID%TYPE,
                           pDetNotaId         IN HOSPITALARIO.SNH_DET_NOTAS.DET_NOTA_ID%TYPE,
                           pTipNotaDetId      IN HOSPITALARIO.SNH_DET_NOTAS.TIPO_DET_NOTA_ID%TYPE,
                           pComentario        IN HOSPITALARIO.SNH_DET_NOTAS.COMENTARIOS%TYPE,
                           pEstadoRegistroId  IN HOSPITALARIO.SNH_DET_NOTAS.ESTADO_REGISTRO_ID%TYPE,   
                           pUsuario           IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE,  
                           pResultado         OUT VARCHAR2,       
                           pMsgError          OUT VARCHAR2) IS  
 vFirma MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_U_DET_NOTAS => ';
 BEGIN
     CASE
     WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ELIMINADO THEN
         <<EliminaRegistro>>
          BEGIN
             UPDATE HOSPITALARIO.SNH_DET_NOTAS
                SET ESTADO_REGISTRO_ID   = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,
                    USUARIO_ELIMINA      = pUsuario,
                    FECHA_ELIMINA        = CURRENT_TIMESTAMP
              WHERE PERSALUD_EVO_NOTA_ID = pPerSaludEvoNotaId AND
                    ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
          EXCEPTION
             WHEN OTHERS THEN
                  pResultado := 'Error no controlado al eliminar registro [Id] - '||pPerSaludEvoNotaId;
                  pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                  RETURN;                
          END EliminaRegistro;
          pResultado := 'Se ha eliminado el registro. [Id:'||pDetNotaId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_PASIVO THEN
         <<PasivaRegistro>>       
         BEGIN
            UPDATE  HOSPITALARIO.SNH_DET_NOTAS
                SET ESTADO_REGISTRO_ID = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,  
                    USUARIO_PASIVA       = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                           ELSE USUARIO_PASIVA
                                           END,    
                    FECHA_PASIVA         = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                           ELSE FECHA_PASIVA
                                           END
             WHERE PERSALUD_EVO_NOTA_ID = pPerSaludEvoNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;     
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al pasivar registro [Id] - '||pPerSaludEvoNotaId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END PasivaRegistro;
         pResultado := 'Se ha pasivado el registro. [Id:'||pDetNotaId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN
         <<ActivarRegistro>>
         BEGIN
            UPDATE HOSPITALARIO.SNH_DET_NOTAS
               SET ESTADO_REGISTRO_ID   = pEstadoRegistroId, 
                   USUARIO_MODIFICACION = pUsuario,    
                   USUARIO_PASIVA       = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                          ELSE USUARIO_PASIVA
                                          END,    
                   FECHA_PASIVA         = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                          ELSE FECHA_PASIVA
                                          END
             WHERE PERSALUD_EVO_NOTA_ID = pPerSaludEvoNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;  
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al activar registro [Id] - '||pPerSaludEvoNotaId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END ActivarRegistro; 
         pResultado := 'Se ha activado el registro. [Id:'||pDetNotaId||']';                        
     ELSE 
         <<ActualizarRegistro>>
         BEGIN
--           CASE
--           WHEN NVL(pIsLast,0) = kES_PRINCIPAL THEN 
--               CASE        
--               WHEN FN_EXISTE_ADMSERV_CAMAS (pAdminServId, pCfgUsalServCamaId) = TRUE THEN
--                    PR_U_ISLAST_ADMSERV_CAMAS (pAdminServId       => pAdminServId,
--                                               pCfgUsalServCamaId => pCfgUsalServCamaId, 
--                                               pUsuario           => pUsuario,
--                                               pResultado         => pResultado,
--                                               pMsgError          => pMsgError);
--                    CASE
--                    WHEN pMsgError IS NOT NULL THEN
--                         pResultado := pResultado;
--                         pMsgError  := pMsgError;
--                         RAISE eSalidaConError;
--                    ELSE NULL;
--                    END CASE;                             
--               ELSE NULL;
--               END CASE;  
--           ELSE NULL;
--           END CASE;       
            UPDATE HOSPITALARIO.SNH_DET_NOTAS  
               SET DET_NOTA_ID          = pDetNotaId,      
                   TIPO_DET_NOTA_ID     = pTipNotaDetId,  
                   COMENTARIOS          = pComentario,    
                   USUARIO_MODIFICACION = pUsuario          
             WHERE PERSALUD_EVO_NOTA_ID = pPerSaludEvoNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;  
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al activar registro [Id] - '||pPerSaludEvoNotaId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END ActualizarRegistro; 
         pResultado := 'Se ha actualizado el registro. [Id:'||pPerSaludEvoNotaId||']';                              
     END CASE;
 EXCEPTION
 WHEN eSalidaConError THEN
      pResultado := pResultado;   
      pMsgError  := vFirma||pMsgError;
 WHEN OTHERS THEN
      pResultado := 'Error no controlado';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM; 
 END PR_U_DET_NOTAS;

 PROCEDURE PR_CRUD_DET_NOTAS (pPerSaludEvoNotaId   IN OUT HOSPITALARIO.SNH_DET_NOTAS.PERSALUD_EVO_NOTA_ID%TYPE,       
