 PROCEDURE PR_U_MST_NOTAS (pDetNotaId           IN HOSPITALARIO.SNH_MST_NOTAS.DET_NOTA_ID%TYPE,
                           pAdmServId           IN HOSPITALARIO.SNH_MST_NOTAS.ADMISION_SERVICIO_ID%TYPE,
                           pFecNota             IN HOSPITALARIO.SNH_MST_NOTAS.FECHA_NOTA%TYPE,
                           pHrNota              IN HOSPITALARIO.SNH_MST_NOTAS.HORA_NOTA%TYPE,
                           pTipNota             IN HOSPITALARIO.SNH_MST_NOTAS.TIPO_NOTA_ID%TYPE,
                           pMpersSaludPrincipal IN HOSPITALARIO.SNH_MST_NOTAS.MPERS_SALUD_ID%TYPE,
                           pEstadoRegistroId    IN HOSPITALARIO.SNH_REL_ADMSRV_CAMAS.ESTADO_REGISTRO_ID%TYPE,
                           pUsuario             IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE,
                           pResultado           OUT VARCHAR2,                               
                           pMsgError            OUT VARCHAR2)IS
 vFirma MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_U_MST_NOTAS => '; 
 BEGIN
     CASE
     WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ELIMINADO THEN
         <<EliminaRegistro>>
          BEGIN
             UPDATE HOSPITALARIO.SNH_MST_NOTAS
                SET ESTADO_REGISTRO_ID   = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,
                    USUARIO_ELIMINA      = pUsuario,
                    FECHA_ELIMINA        = CURRENT_TIMESTAMP
              WHERE DET_NOTA_ID = pDetNotaId AND
                    ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
          EXCEPTION
             WHEN OTHERS THEN
                  pResultado := 'Error no controlado al eliminar registro [Id] - '||pDetNotaId;
                  pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                  RETURN;                
          END EliminaRegistro;
          pResultado := 'Se ha eliminado el registro. [Id:'||pDetNotaId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_PASIVO THEN
         <<PasivaRegistro>>       
         BEGIN
            UPDATE  HOSPITALARIO.SNH_MST_NOTAS
                SET ESTADO_REGISTRO_ID = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,  
                    USUARIO_PASIVA       = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                           ELSE USUARIO_PASIVA
                                           END,    
                    FECHA_PASIVA         = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                           ELSE FECHA_PASIVA
                                           END
             WHERE DET_NOTA_ID = pDetNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;     
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al pasivar registro [Id] - '||pDetNotaId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END PasivaRegistro;
         pResultado := 'Se ha pasivado el registro. [Id:'||pDetNotaId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN
         <<ActivarRegistro>>
         BEGIN
            UPDATE HOSPITALARIO.SNH_MST_NOTAS
               SET ESTADO_REGISTRO_ID   = pEstadoRegistroId, 
                   USUARIO_MODIFICACION = pUsuario,    
                   USUARIO_PASIVA       = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                          ELSE USUARIO_PASIVA
                                          END,    
                   FECHA_PASIVA         = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                          ELSE FECHA_PASIVA
                                          END
             WHERE DET_NOTA_ID = pDetNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;  
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al activar registro [Id] - '||pDetNotaId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END ActivarRegistro; 
         pResultado := 'Se ha activado el registro. [Id:'||pDetNotaId||']';                        
     ELSE 
         <<ActualizarRegistro>>
         BEGIN
--           CASE
--           WHEN NVL(pIsLast,0) = kES_PRINCIPAL THEN 
--               CASE        
--               WHEN FN_EXISTE_ADMSERV_CAMAS (pAdminServId, pCfgUsalServCamaId) = TRUE THEN
--                    PR_U_ISLAST_ADMSERV_CAMAS (pAdminServId       => pAdminServId,
--                                               pCfgUsalServCamaId => pCfgUsalServCamaId, 
--                                               pUsuario           => pUsuario,
--                                               pResultado         => pResultado,
--                                               pMsgError          => pMsgError);
--                    CASE
--                    WHEN pMsgError IS NOT NULL THEN
--                         pResultado := pResultado;
--                         pMsgError  := pMsgError;
--                         RAISE eSalidaConError;
--                    ELSE NULL;
--                    END CASE;                             
--               ELSE NULL;
--               END CASE;  
--           ELSE NULL;
--           END CASE;       
            UPDATE HOSPITALARIO.SNH_MST_NOTAS  
               SET ADMISION_SERVICIO_ID = NVL(pAdmServId, ADMISION_SERVICIO_ID),              
                   FECHA_NOTA           = NVL(pFecNota, FECHA_NOTA),                         
                   HORA_NOTA            = NVL(pHrNota, HORA_NOTA),                         
                   MPERS_SALUD_ID       = NVL(pMpersSaludPrincipal, MPERS_SALUD_ID),        
                   TIPO_NOTA_ID         = NVL(pTipNota, TIPO_NOTA_ID),
                   USUARIO_MODIFICACION = pUsuario          
             WHERE DET_NOTA_ID = pDetNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;  
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al activar registro [Id] - '||pDetNotaId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END ActualizarRegistro; 
         pResultado := 'Se ha actualizado el registro. [Id:'||pDetNotaId||']';                              
     END CASE;
 EXCEPTION
 WHEN eSalidaConError THEN
      pResultado := pResultado;   
      pMsgError  := vFirma||pMsgError;
 WHEN OTHERS THEN
      pResultado := 'Error no controlado';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM; 
 END PR_U_MST_NOTAS; 

 FUNCTION FN_VAL_EXISTE_MST_NOTAS (pConsulta      IN HOSPITALARIO.OBJ_MST_NOTAS,
                                   pPgn           IN BOOLEAN, 
                                   pCantRegistros OUT NUMBER,  
                                   pFuente        OUT NUMBER) RETURN BOOLEAN AS
 vRetorna BOOLEAN := FALSE;
 vContador SIMPLE_INTEGER := 0; 
 BEGIN
     CASE
     WHEN NVL(pConsulta.detNotaId,0) > 0 AND
          NVL(pConsulta.persaludEvoNotaId,0) > 0 THEN     
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS A
              JOIN HOSPITALARIO.SNH_DET_NOTAS B
                ON B.DET_NOTA_ID = A.DET_NOTA_ID
               AND B.PERSALUD_EVO_NOTA_ID = pConsulta.persaludEvoNotaId
             WHERE A.DET_NOTA_ID = pConsulta.detNotaId AND
                   A.ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 1;
            END; 
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador);     
     WHEN NVL(pConsulta.detNotaId,0) > 0 THEN
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS
             WHERE DET_NOTA_ID = pConsulta.detNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 2;
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador); 
            END; 
     WHEN NVL(pConsulta.persaludEvoNotaId,0) > 0 THEN     
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_DET_NOTAS B
             WHERE B.PERSALUD_EVO_NOTA_ID = pConsulta.persaludEvoNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 3;
            END;         
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador);     
     WHEN NVL(pConsulta.AdminServId,0) > 0 AND 
          NVL(pConsulta.mperSaludId,0) > 0 AND
          NVL(pConsulta.tipoNotaId,0) > 0 AND
              pConsulta.fechaNota IS NOT NULL THEN
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS
             WHERE ADMISION_SERVICIO_ID = pConsulta.AdminServId AND
                   MPERS_SALUD_ID       = pConsulta.mperSaludId AND
                   TIPO_NOTA_ID         = pConsulta.tipoNotaId AND
                   FECHA_NOTA           = pConsulta.fechaNota AND 
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 4;
            END;      
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador); 
     WHEN NVL(pConsulta.AdminServId,0) > 0 AND 
          NVL(pConsulta.mperSaludId,0) > 0 AND
          NVL(pConsulta.tipoNotaId,0) > 0 THEN
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS
             WHERE ADMISION_SERVICIO_ID = pConsulta.AdminServId AND
                   MPERS_SALUD_ID       = pConsulta.mperSaludId AND
                   TIPO_NOTA_ID         = pConsulta.tipoNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 5;
            END; 
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador);  
     WHEN NVL(pConsulta.AdminServId,0) > 0 AND 
          NVL(pConsulta.mperSaludId,0) > 0 AND
              pConsulta.fechaNota IS NOT NULL THEN
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS
             WHERE ADMISION_SERVICIO_ID = pConsulta.AdminServId AND
                   MPERS_SALUD_ID       = pConsulta.mperSaludId AND
                   FECHA_NOTA           = pConsulta.fechaNota AND 
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 6;
            END;   
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador); 
     WHEN NVL(pConsulta.AdminServId,0) > 0 AND 
          NVL(pConsulta.mperSaludId,0) > 0 THEN
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS
             WHERE ADMISION_SERVICIO_ID = pConsulta.AdminServId AND
                   MPERS_SALUD_ID       = pConsulta.mperSaludId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 7;
            END; 
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador); 
     WHEN NVL(pConsulta.AdminServId,0) > 0 AND 
          NVL(pConsulta.tipoNotaId,0) > 0 THEN
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS
             WHERE ADMISION_SERVICIO_ID = pConsulta.AdminServId AND
                   TIPO_NOTA_ID         = pConsulta.tipoNotaId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 8;
            END; 
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador); 
     WHEN NVL(pConsulta.AdminServId,0) > 0 AND 
              pConsulta.fechaNota IS NOT NULL THEN
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS
             WHERE ADMISION_SERVICIO_ID = pConsulta.AdminServId AND
                   FECHA_NOTA           = pConsulta.fechaNota AND 
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 9;
            END; 
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador); 
     WHEN NVL(pConsulta.AdminServId,0) > 0 THEN
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS
             WHERE ADMISION_SERVICIO_ID = pConsulta.AdminServId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 10;
            END;     
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador);                                 
     ELSE 
            BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_NOTAS
             WHERE ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 11;
            END;
            DBMS_OUTPUT.PUT_LINE ('fuente: '||pFuente||' / contador: '||vContador);       
     END CASE;
     CASE
     WHEN vContador > 0 THEN
          vRetorna := TRUE;
     ELSE NULL;
     END CASE;
     pCantRegistros := vContador;
     RETURN vRetorna;
 EXCEPTION
 WHEN OTHERS THEN
      RETURN vRetorna;     
 END FN_VAL_EXISTE_MST_NOTAS; 


 FUNCTION FN_OBT_MST_NOTAS_DET_ID (pDetNotaId         IN HOSPITALARIO.SNH_MST_NOTAS.DET_NOTA_ID%TYPE,
                                   pPerSaludEvoNotaId IN HOSPITALARIO.SNH_DET_NOTAS.PERSALUD_EVO_NOTA_ID%TYPE) RETURN var_refcursor AS
 vRegistro var_refcursor;
 BEGIN
