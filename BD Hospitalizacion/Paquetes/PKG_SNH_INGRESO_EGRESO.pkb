                                pTipoDiagnostico IN CATALOGOS.SBC_CAT_CATALOGOS.CODIGO%TYPE,
                                pUsuario         IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE,
                                pAccionEstado    IN VARCHAR2,                                
                                pFecInicio       IN DATE,                                    
                                pFecFin          IN DATE,                                    
                                pJson            IN VARCHAR2,          
                                pTipoAccion      IN VARCHAR2,          
                                pPgn             IN NUMBER,            
                                pPgnAct          IN NUMBER default 1,  
                                pPgnTmn          IN NUMBER default 100,
                                pDatosPaginacion OUT var_refcursor,    
                                pRegistro        OUT var_refcursor,    
                                pResultado       OUT VARCHAR2,         
                                pMsgError        OUT VARCHAR2) IS
 vFirma             MAXVARCHAR2 := kPAQUETE_FIRMA||'PR_CRUD_DIAGNOSTICO => ';
 vResultado         MAXVARCHAR2;
 vEstadoRegistroId  CATALOGOS.SBC_CAT_CATALOGOS.CATALOGO_ID%TYPE;  
 vIdJson            HOSPITALARIO.SNH_JSON_DATA.ID%TYPE;
 
 vRegDxRelIngEg     refDxRelIngEg;
 vVarRegDxRelIngEg  DIAGNOSTICOS_REL_ING_EGR;

 vExisteDetalle     SIMPLE_INTEGER := 0;
 
 vDxId              HOSPITALARIO.SNH_MST_DIAGNOSTICOS.DIAGNOSTICO_ID%TYPE;  

 vRelDxIngEgId      SNH_REL_DIAGNOSTICOS_ING_EG.REL_DX_ING_EG_ID%TYPE;   
 
 vJson              BOOLEAN := FALSE;                     
 BEGIN
      CASE
      WHEN pTipoAccion IS NULL THEN 
           pResultado := 'El párametro pTipoAccion no puede venir NULL';
           pMsgError  := pResultado;
           RAISE eParametroNull;
      ELSE NULL;
      END CASE;
      CASE
      WHEN pTipoAccion = kINSERT THEN
           CASE
           WHEN pUsuario IS NULL THEN
                pResultado := 'El usuario no puede venir nulo.';
                pMsgError  := pResultado;
                RAISE eParametroNull;
           ELSE 
                CASE -- validamos que el usuario se valido, con mst_usuarios
                WHEN (PKG_SNH_UTILITARIOS.FN_VALIDAR_USUARIO (pUsuario)) = FALSE THEN
                      pResultado := 'Usuario no valido';
                      pMsgError  := pResultado;
                      RAISE eRegistroNoExiste;
                ELSE NULL;
                END CASE;
           END CASE;
           dbms_output.put_line ('Sale de validar usuario');
           CASE
           WHEN pJson IS NOT NULL THEN
                dbms_output.put_line ('Trae parámetro json');
                PR_I_JSON_DATA (pIdJson    => vIdJson,
                                pJson      => pJson,
                                pResultado => pResultado,           
                                pMsgError  => pMsgError); 
                    dbms_output.put_line ('vIdJson: '||vIdJson); 
                    dbms_output.put_line ('Saliendo de json pMsgError: '||pMsgError);             
                    CASE
                    WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                         RAISE eSalidaConError;
                    ELSE 
                        PR_C_JSON_DATA_DIAGNOSTICOS (pIdJson    => vIdJson,
                                                     pRegistro  => vRegDxRelIngEg,  --pRegistro,
                                                     pResultado => pResultado,
                                                     pMsgError  => pMsgError);
                                 dbms_output.put_line ('Saliendo de PR_C_JSON_DATA_NOTAS pMsgError: '||pMsgError);                 
                                CASE
                                WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                                     RAISE eSalidaConError;
                                ELSE 
                                     dbms_output.put_line ('antes de asignar variables de cursor');
                                     LOOP
                                     FETCH vRegDxRelIngEg INTO vVarRegDxRelIngEg;
                                     vExisteDetalle := vRegDxRelIngEg%ROWCOUNT;
                                     EXIT WHEN vRegDxRelIngEg%NOTFOUND; 
                                          PR_I_DIAGNOSTICOS (pDxId                 => vDxId,
                                                             pAdminServId          => vVarRegDxRelIngEg.admisionServId,
                                                             pMpersSaludId         => vVarRegDxRelIngEg.mpersSaludId,
                                                             pCieId                => vVarRegDxRelIngEg.cieId,
                                                             pMperSaludCieCodifica => vVarRegDxRelIngEg.usrCodificaCie,
                                                             pTipDxCod             => vVarRegDxRelIngEg.TipoDiagnostico,
                                                             pDiagnostico          => vVarRegDxRelIngEg.diagnostico,
                                                             pUsuario              => pUsuario,
                                                             pResultado            => pResultado,
                                                             pMsgError             => pMsgError);
                                          CASE
                                          WHEN pMsgError IS NOT NULL THEN
                                               RAISE eSalidaConError;
                                          ELSE
                                              PR_CRUD_REL_DX_ING_EG (pRelDxIngEgId    => vRelDxIngEgId, 
                                                                     pIngresoId       => vVarRegDxRelIngEg.ingresoId, 
                                                                     pTipoDxIngEg     => vVarRegDxRelIngEg.tipoDxCodigo,  
                                                                     pDxId            => vDxId,  
                                                                     pTrasladoId      => NULL, 
                                                                     pUsuario         => pUsuario,  
                                                                     pAccionEstado    => NULL, 
                                                                     pFecInicio       => NULL, 
                                                                     pFecFin          => NULL, 
                                                                     pJson            => NULL, 
                                                                     pTipoAccion      => kINSERT, 
