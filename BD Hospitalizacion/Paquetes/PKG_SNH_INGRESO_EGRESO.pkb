                       dbms_output.put_line ('vVarRegDxRelIngEg.usrCodificaCie: '||vVarRegDxRelIngEg.usrCodificaCie);
                       dbms_output.put_line ('vVarRegDxRelIngEg.TipoDiagnostico: '||vVarRegDxRelIngEg.TipoDiagnostico);
                       dbms_output.put_line ('vVarRegDxRelIngEg.diagnostico: '||vVarRegDxRelIngEg.diagnostico);
                       PR_U_DIAGNOSTICOS (pDxId                 => vVarRegDxRelIngEg.diagnosticoId,
                                          pAdminServId          => vVarRegDxRelIngEg.admisionServId,
                                          pMpersSaludId         => vVarRegDxRelIngEg.mpersSaludId,
                                          pCieId                => vVarRegDxRelIngEg.cieId,
                                          pMperSaludCieCodifica => vVarRegDxRelIngEg.usrCodificaCie,
