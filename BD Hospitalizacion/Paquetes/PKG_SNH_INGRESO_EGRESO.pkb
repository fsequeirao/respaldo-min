                                                                     pDatosPaginacion => pDatosPaginacion, 
                                                                     pRegistro        => pRegistro,   
                                                                     pResultado       => pResultado, 
                                                                     pMsgError        => pMsgError); 
                                              CASE
                                              WHEN pMsgError IS NOT NULL THEN
                                                  RAISE eSalidaConError;
                                              ELSE NULL;
                                              END CASE;                           
                                          END CASE;
                                     END LOOP;        
                                     CLOSE vRegDxRelIngEg;
                                     dbms_output.put_line ('despues de asignar variables de cursor');
                                     
                                     PR_C_DIAGNOSTICOS (pConsulta       => HOSPITALARIO.OBJ_DIAGNOSTICOS (
                                                                                                         diagnosticoId => null,
                                                                                                         RelDxIngEgId  => null,
                                                                                                         IngresoId     => vVarRegDxRelIngEg.ingresoId,
                                                                                                         TrasladoId    => null,
                                                                                                         AdminServId   => null,
                                                                                                         mperSaludId   => null,
                                                                                                         fechaSolucion => null,
                                                                                                         TipoDxId      => null,
                                                                                                         TipoDxIngEg   => null,
                                                                                                         FecInicio     => null,
                                                                                                         FecFin        => null
                                                                                                         ),
                                                        pPgn             => pPgn,            
                                                        pPgnAct          => pPgnAct,          
                                                        pPgnTmn          => pPgnTmn,         
                                                        pDatosPaginacion => pDatosPaginacion,
                                                        pRegistro        => pRegistro,       
                                                        pResultado       => pResultado,
                                                        pMsgError        => pMsgError);
                                     CASE
                                     WHEN pMsgError IS NOT NULL THEN
                                         RAISE eSalidaConError;
                                     ELSE vResultado := 'Se crean exitosamente registros de diagnosticos para el ingreso id: '||vVarRegDxRelIngEg.ingresoId||', devolviendo el JSon de este';
                                     END CASE;                                                         
                                END CASE;                        
                    END CASE;
                    vIdJson := null;                                 
           ELSE NULL;
           END CASE;
      WHEN pTipoAccion = kUPDATE THEN
           CASE
           WHEN pUsuario IS NULL THEN
                pResultado := 'El usuario no puede venir nulo.';
                pMsgError  := pResultado;
                RAISE eParametroNull;
           ELSE 
                CASE -- validamos que el usuario se valido, con mst_usuarios
