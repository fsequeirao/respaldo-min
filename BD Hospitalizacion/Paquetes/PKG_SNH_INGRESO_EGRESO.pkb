                                  pTrasladoId      IN HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG.TRASLADO_ID%TYPE,
                                  pUsuario         IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE,
                                  pAccionEstado    IN VARCHAR2,                                
                                  pFecInicio       IN DATE,                                    
                                  pFecFin          IN DATE,                                    
                                  pJson            IN VARCHAR2,          
                                  pTipoAccion      IN VARCHAR2,          
                                  pPgn             IN NUMBER,            
