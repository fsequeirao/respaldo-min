                         pEstadoRegistroId  IN HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.ESTADO_REGISTRO_ID%TYPE,
                         pUsuario           IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE,
                         pResultado         OUT VARCHAR2,
                         pMsgError          OUT VARCHAR2) IS
                         
 vFirma MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_U_INGRESO => ';
 vUsuarioRegEgreso SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE;
 vFecRegEgreso     HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.FECHA_REGISTRO_EGRESO%TYPE; 
 vTipoEgresoId     HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.TIPO_EGRESO_ID%TYPE;
 vFecFinIngreso    HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.FECHA_FIN_INGRESO%TYPE;
 vHrFinIngreso     HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.HORA_FIN_INGRESO%TYPE;
 vServEgresoId     HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.SERVICIO_EGRESO_ID%TYPE;
 vMedEgresoId      HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.MEDICO_EGRESO_ID%TYPE;
 vEstadoPxEgreId   HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.ESTADO_PX_EGRESO_ID%TYPE; 
 vEstadoIngreso    HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS.ESTADO_INGRESO_ID%TYPE;
 
 vRegPregIngresos refPregIngresos; 
 vVarPregIngresos HOSPITALARIO.SNH_MST_PREG_INGRESOS%ROWTYPE;
 
 vRegIngrEgr      refIngEgreso; 
 vVarRegIngrEgr   HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS%ROWTYPE;
  
 vRegistro        var_refcursor; 
 vDatosPaginacion var_refcursor;
 
 vPregIngresoId   NUMBER:= pPregIngresoId;
 
 vEstPreIngresoPendienteId NUMBER := HOSPITALARIO.PKG_SNH_UTILITARIOS.FN_OBT_CATALOGO_ID ('STSLPRG', 'STSLPRG|PND'); 
 
 vRegCadminRelCamaServ    refCadminRelCamaServ;
 vVarRegCadminRelCamaServ HOSPITALARIO.SNH_REL_ADMSRV_CAMAS%ROWTYPE;
 
 vAdminServCamaId HOSPITALARIO.SNH_REL_ADMSRV_CAMAS.ADMISION_SRV_CAMA_ID%TYPE;
 BEGIN
     CASE
     WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ELIMINADO THEN
         <<EliminaRegistro>>
          BEGIN
             UPDATE HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS
                SET ESTADO_REGISTRO_ID   = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,
                    USUARIO_ELIMINA      = pUsuario,
                    FECHA_ELIMINA        = CURRENT_TIMESTAMP
              WHERE INGRESO_ID = pIngresoId;
              
                 vRegIngrEgr :=  FN_OBT_COL_ING_EGR (pIngresoId);
                 dbms_output.put_line ('Luego de FN_OBT_COL_ING_EGR');
                 FETCH vRegIngrEgr INTO vVarRegIngrEgr;
                 CLOSE vRegIngrEgr;              
              
              
                 vRegPregIngresos :=  FN_OBT_COL_PREG_ING (vVarRegIngrEgr.PREG_INGRESO_ID);
                 FETCH vRegPregIngresos INTO vVarPregIngresos;
                 CLOSE vRegPregIngresos;                 
                 PR_CRUD_PRE_INGRESO (pPregIngresoId     => vVarRegIngrEgr.PREG_INGRESO_ID,                  
                                      pAdmisionId        => vVarPregIngresos.ADMISION_ID,                         
                                      pProcedenciaId     => vVarPregIngresos.PROCEDENCIA_ID,                         
                                      pPerNominalId      => vVarPregIngresos.PER_NOMINAL_ID,                         
                                      pCodExpElectronico => vVarPregIngresos.CODIGO_EXPEDIENTE_ELECTRONICO,                         
                                      pExpedienteId      => vVarPregIngresos.EXPEDIENTE_ID,                         
                                      pNomCompletoPx     => vVarPregIngresos.NOMBRE_COMPLETO_PX,                         
                                      pMedOrdenaIngId    => vVarPregIngresos.MEDICO_ORDENA_INGRESO_ID,                         
                                      pUsalOrigenId      => vVarPregIngresos.UNIDAD_SALUD_ORIGEN_ID,                         
                                      pServProcedenId    => vVarPregIngresos.SERVICIO_PROCEDENCIA_ID,                         
                                      pAdminSolicIngId   => vVarPregIngresos.ADMISIONISTA_SOLICITA_INGR_ID,                         
                                      pFecSolicitaIng    => vVarPregIngresos.FECHA_SOLICITUD_INGRESO,                         
                                      pHrSolicitudIng    => vVarPregIngresos.HORA_SOLICITUD_INGRESO,                         
                                      pUsalDestinoId     => vVarPregIngresos.UNIDAD_SALUD_DESTINO_ID,                         
                                      pReferenciaId      => vVarPregIngresos.REFERENCIA_ID,                         
                                      pEspDestinoId      => vVarPregIngresos.ESPECIALIDAD_DESTINO_ID,                         
                                      pEstadoPreIngId    => vEstPreIngresoPendienteId,    -- vGLOBAL_ESTPREING_INGRESADO,  
                                      pComentarios       => vVarPregIngresos.COMENTARIOS,                         
                                      pTipIdentiId       => vVarPregIngresos.TIPO_IDENTIFICACION_ID,                         
                                      pIdentificacion    => vVarPregIngresos.IDENTIFICACION,                         
                                      pEstadoPxId        => vVarPregIngresos.ESTADO_PX_ID,                         
                                      pUsuario           => pUsuario,
                                      pFecInicio         => NULL, 
                                      pFecFin            => NULL,
                                      pAccionEstado      => null,     
                                      pTipoAccion        => kUPDATE, 
                                      pPgn               => null,         
                                      pDatosPaginacion   => vDatosPaginacion,
                                      pRegistro          => vRegistro,
                                      pResultado         => pResultado,
                                      pMsgError          => pMsgError);

                CASE 
                WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                     RAISE eSalidaConError;
                ELSE NULL;
                END CASE;   
               --- Se manda a actualizar el estado de admisión servcio a eliminar
                dbms_output.put_line ('antes de SNH_CRUD_ADMISION_SERVICIO: admission: '||vVarRegIngrEgr.ADMISION_SERVICIO_ID);
                HOSPITALARIO.PKG_SNH_CONSULTA_EXTERNA.SNH_CRUD_ADMISION_SERVICIO (pUsrName        => pUsuario,               
                                                                                  pAdmisionServId => vVarRegIngrEgr.ADMISION_SERVICIO_ID,               
                                                                                  pAdmisionId     => NULL,               
                                                                                  pServicioId     => NULL,               
                                                                                  pMPerSaludId    => NULL,               
                                                                                  pLugarAtencion  => NULL,               
                                                                                  pDependenciaId  => NULL,               
                                                                                  pFechaIni       => NULL,               
                                                                                  pFechaFin       => NULL,               
                                                                                  pEstado         => vGLOBAL_ESTADO_ELIMINADO,               
                                                                                  pEsPrincipal    => NULL,               
                                                                                  pEspecialidad   => NULL,               
                                                                                  pTipoOperacion  => kUPDATE,               
                                                                                  pRegistro       => vRegistro,              
                                                                                  pResultado      => pResultado,         
                                                                                  pMsgError       => pMsgError);            
                dbms_output.put_line ('despues de SNH_CRUD_ADMISION_SERVICIO: error: '||pMsgError);
                CASE 
                WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                     RAISE eSalidaConError;
                ELSE NULL;
                END CASE;
                
                CASE
                WHEN OBT_EXISTE_ADMIN_SRV_CAMA_ID (vVarRegIngrEgr.ADMISION_SERVICIO_ID) = TRUE THEN
                     vAdminServCamaId := OBT_ADMISION_SRV_CAMA_ID (vVarRegIngrEgr.ADMISION_SERVICIO_ID);
                     dbms_output.put_line ('vAdminServCamaId: '||vAdminServCamaId);
                     CASE
                     WHEN NVL(vAdminServCamaId,0) > 0 THEN
                          dbms_output.put_line ('Validación de existe vAdminServCamaId: '||vAdminServCamaId);
                          vRegCadminRelCamaServ := FN_OBT_COL_ADMI_REL_SERV_CAMAS (vAdminServCamaId);
                          FETCH vRegCadminRelCamaServ INTO vVarRegCadminRelCamaServ;
                          CLOSE vRegCadminRelCamaServ;
                          dbms_output.put_line ('CFG_USLD_SERVICIO_CAMA_ID: '||vVarRegCadminRelCamaServ.CFG_USLD_SERVICIO_CAMA_ID);
                          PR_CRUD_REL_ADMSRV_CAMAS (pAdminServCamaId   => vAdminServCamaId,
                                                    pCfgUsalServCamaId => vVarRegCadminRelCamaServ.CFG_USLD_SERVICIO_CAMA_ID,
                                                    pAdminServId       => null,  -- vVarRegIngrEgr.ADMISION_SERVICIO_ID,
                                                    pFechaInicio       => null,  -- vVarRegCadminRelCamaServ.FECHA_INI,
                                                    pHoraInicio        => null,  -- vVarRegCadminRelCamaServ.HORA_INI,  
                                                    pFechaFin          => null,  -- vFecFinIngreso,
                                                    pHoraFin           => null,  -- vHrFinIngreso, 
                                                    pIsLast            => null,  -- vVarRegCadminRelCamaServ.IS_LAST,   
                                                    pUsalud            => null,
                                                    pUsuario           => pUsuario,  
                                                    pAccionEstado      => NULL,                                  
                                                    pTipoAccion        => kDELETE,                                  
                                                    pPgn               => NULL,
                                                    pDatosPaginacion   => vDatosPaginacion,
                                                    pRegistro          => vRegistro,                           
                                                    pResultado         => pResultado,                                
                                                    pMsgError          => pMsgError);
                          dbms_output.put_line ('despues de PR_CRUD_REL_ADMSRV_CAMAS: error: '||pMsgError);                          
                          CASE 
                          WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                               RAISE eSalidaConError;
                          ELSE NULL;
                          END CASE;                                                    
                     ELSE NULL;
                     END CASE;
                ELSE NULL;
                END CASE;                                            
          EXCEPTION
             WHEN OTHERS THEN
                  pResultado := 'Error no controlado al eliminar registro [pIngresoId] - '||pIngresoId;
                  pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                  RETURN;                
          END EliminaRegistro;
          pResultado := 'Se ha eliminado el registro. [Id:'||pIngresoId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_PASIVO THEN
         <<PasivaRegistro>>       
         BEGIN
            UPDATE  HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS
                SET ESTADO_REGISTRO_ID = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,  
                    USUARIO_PASIVA       = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                           ELSE USUARIO_PASIVA
                                           END,    
                    FECHA_PASIVA         = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                           ELSE FECHA_PASIVA
                                           END
             WHERE INGRESO_ID = pIngresoId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;     
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al pasivar registro [pIngresoId] - '||pIngresoId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END PasivaRegistro;
         pResultado := 'Se ha pasivado el registro. [Id:'||pIngresoId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN
          <<ActivarRegistro>>
         BEGIN
            UPDATE HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS
               SET ESTADO_REGISTRO_ID   = pEstadoRegistroId, 
                   USUARIO_MODIFICACION = pUsuario,    
                   USUARIO_PASIVA       = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                          ELSE USUARIO_PASIVA
                                          END,    
                   FECHA_PASIVA         = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                          ELSE FECHA_PASIVA
                                          END
             WHERE INGRESO_ID = pIngresoId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;  
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al activar registro [pIngresoId] - '||pIngresoId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END ActivarRegistro; 
         pResultado := 'Se ha activado el registro. [Id:'||pIngresoId||']';                        
     ELSE 
        CASE
        WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN
            CASE
            WHEN pFecFinIngreso IS NULL THEN
                 vUsuarioRegEgreso := NULL; 
                 vFecRegEgreso     := NULL;
                 vTipoEgresoId     := NULL;
                 vFecFinIngreso    := NULL;
                 vHrFinIngreso     := NULL;
                 vServEgresoId     := NULL;
                 vMedEgresoId      := NULL;
                 vEstadoPxEgreId   := NULL;
                 vEstadoIngreso    := vGLOBAL_ESTPREING_INGRESADO;
            ELSE 
                 vUsuarioRegEgreso := pUsuario; 
                 vFecRegEgreso     := SYSDATE;
                 vTipoEgresoId     := pTipoEgresoId;
                 vFecFinIngreso    := pFecFinIngreso;
                 vHrFinIngreso     := pHrFinIngreso; 
                 vServEgresoId     := pServEgresoId;  
                 vMedEgresoId      := pMedicoEgresoId; 
                 vEstadoPxEgreId   := pEstadoPxEgresoId;  
                 vEstadoIngreso    := vGLOBAL_ESTPREING_EGRESADO;       
            END CASE;
        ELSE NULL;
        END CASE;     
         <<ActualizarRegistro>>
         BEGIN
            UPDATE HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS  
               SET PREG_INGRESO_ID               = NVL(pPregIngresoId,PREG_INGRESO_ID),     
                   PER_NOMINAL_ID                = NVL(pPerNominalId,PER_NOMINAL_ID),     
                   PROCEDENCIA_ID                = NVL(pProcedenciaId, PROCEDENCIA_ID),    
                   ADMISION_SERVICIO_ID          = NVL(pAdmisionId, ADMISION_SERVICIO_ID),       
                   EDAD_EXACTA_INGRESO           = NVL(pEdadExactaIng,EDAD_EXACTA_INGRESO),    
                   GRUPO_ETAREO_ID               = NVL(pGrupoEtareoId,GRUPO_ETAREO_ID),    
                   MEDICO_INGRESO_ID             = NVL(pMedicoIngId, MEDICO_INGRESO_ID),      
                   ADMISIONISTA_SOLICITA_INGR_ID = NVL(pAdminSolicIngId, ADMISIONISTA_SOLICITA_INGR_ID),  
                   ADMISIONISTA_INGRESO_ID       = NVL(pAdmisionistaIngId,ADMISIONISTA_INGRESO_ID),
                   MEDICO_ORDENA_INGRESO_ID      = NVL(pMedOrdenaIngId,MEDICO_ORDENA_INGRESO_ID),   
                   SERVICIO_PROCEDENCIA_ID       = NVL(pServProcedenId,SERVICIO_PROCEDENCIA_ID),   
                   REINGRESO                     = NVL(pReingreso,REINGRESO),        
                   REINGRESO_ID                  = pReingresoId,      
                   FECHA_SOLICITUD_INGRESO       = NVL(pFecSolicitaIng,FECHA_SOLICITUD_INGRESO),   
                   HORA_SOLICITUD_INGRESO        = NVL(pHrSolicitudIng,HORA_SOLICITUD_INGRESO),   
                   FECHA_INICIO_INGRESO          = NVL(pFecInicioIngreso,FECHA_INICIO_INGRESO), 
                   HORA_INICIO_INGRESO           = NVL(pHrInicioIngreso,HORA_INICIO_INGRESO),  
                   UNIDAD_SALUD_INGRESO          = NVL(pUsalIngresoId,UNIDAD_SALUD_INGRESO),    
                   SERVICIO_INGRESO_ID           = NVL(pServIngresoId, SERVICIO_INGRESO_ID),    
                   ESTADO_INGRESO_ID             = CASE
                                                   WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN -- NVL(vTipoEgresoId,TIPO_EGRESO_ID),         -- pTipoEgresoId,
                                                        vEstadoIngreso
                                                   ELSE NVL(pEstadoIngId,ESTADO_INGRESO_ID)
                                                   END, --NVL(pEstadoIngId,ESTADO_INGRESO_ID),      
                   TIPO_EGRESO_ID                = CASE
                                                   WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN -- NVL(vTipoEgresoId,TIPO_EGRESO_ID),         -- pTipoEgresoId,
                                                        vTipoEgresoId
                                                   END,     
                   FECHA_FIN_INGRESO             = CASE
                                                   WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN
                                                        vFecFinIngreso
                                                   END,                                    -- NVL(vFecFinIngreso, FECHA_FIN_INGRESO),    -- pFecFinIngreso,    
                   HORA_FIN_INGRESO              = CASE
                                                   WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN
                                                        vHrFinIngreso
                                                   END,                                    -- NVL(vHrFinIngreso, HORA_FIN_INGRESO ),      -- pHrFinIngreso,     
                   SERVICIO_EGRESO_ID            = CASE
                                                   WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN
                                                        vServEgresoId
                                                   END,                                    -- NVL(vServEgresoId, SERVICIO_EGRESO_ID),     -- pServEgresoId,     
                   MEDICO_EGRESO_ID              = CASE
                                                   WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN
                                                        vMedEgresoId
                                                   END,                                    -- NVL(vMedEgresoId, MEDICO_EGRESO_ID),       -- pMedicoEgresoId,   
                   REFERENCIA_ID                 = pReferenciaId,     
                   ES_CONTRAFERIDO               = NVL(pEsContraferido,ES_CONTRAFERIDO),   
                   ENVIO_CONTRAREFERENCIA_ID     = pEnvContrareferId, 
                 --  DIAS_ESTANCIA                 = pDiasEstancia,     
                   USUARIO_REGISTRO_EGRESO       = CASE
                                                   WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN
                                                        vUsuarioRegEgreso
                                                   END, 
                   FECHA_REGISTRO_EGRESO         = CASE
                                                   WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN
                                                        vFecRegEgreso
                                                   END,                                                    
                   ESTADO_PX_ID                  = NVL(pEstadoPxId,ESTADO_PX_ID),       
                   ESTADO_PX_EGRESO_ID           = CASE
                                                   WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN
                                                        vEstadoPxEgreId
                                                   END,                                   -- NVL(vEstadoPxEgreId, ESTADO_PX_EGRESO_ID),                    -- pEstadoPxEgresoId, 
                   COMENTARIOS                   = pComentarios,
                   USUARIO_MODIFICACION          = pUsuario
             WHERE INGRESO_ID = pIngresoId AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;  
                   
                CASE
                WHEN NVL(pTipIngEgr,'X') = kEGRESO THEN   
                     vRegIngrEgr :=  FN_OBT_COL_ING_EGR (pIngresoId);
                     dbms_output.put_line ('Luego de FN_OBT_COL_ING_EGR. ultimo update ingreso');
                     FETCH vRegIngrEgr INTO vVarRegIngrEgr;
                     CLOSE vRegIngrEgr;  
                 
                CASE
                WHEN OBT_EXISTE_ADMIN_SRV_CAMA_ID (vVarRegIngrEgr.ADMISION_SERVICIO_ID) = TRUE THEN
                     vAdminServCamaId := OBT_ADMISION_SRV_CAMA_ID (vVarRegIngrEgr.ADMISION_SERVICIO_ID);
                     CASE
                     WHEN NVL(vAdminServCamaId,0) > 0 THEN
                          FETCH vRegCadminRelCamaServ INTO vVarRegCadminRelCamaServ;
                          CLOSE vRegCadminRelCamaServ;
                          PR_CRUD_REL_ADMSRV_CAMAS (pAdminServCamaId   => vAdminServCamaId,
                                                    pCfgUsalServCamaId => vVarRegCadminRelCamaServ.CFG_USLD_SERVICIO_CAMA_ID,
                                                    pAdminServId       => vVarRegIngrEgr.ADMISION_SERVICIO_ID,
                                                    pFechaInicio       => vVarRegCadminRelCamaServ.FECHA_INI,
                                                    pHoraInicio        => vVarRegCadminRelCamaServ.HORA_INI,  
                                                    pFechaFin          => vFecFinIngreso,
                                                    pHoraFin           => vHrFinIngreso, 
                                                    pIsLast            => vVarRegCadminRelCamaServ.IS_LAST,   
                                                    pUsalud            => null,
                                                    pUsuario           => pUsuario,  
                                                    pAccionEstado      => NULL,                                  
                                                    pTipoAccion        => kUPDATE,                                  
                                                    pPgn               => NULL,
                                                    pDatosPaginacion   => vDatosPaginacion,
                                                    pRegistro          => vRegistro,                           
                                                    pResultado         => pResultado,                                
                                                    pMsgError          => pMsgError);
                     ELSE NULL;
                     END CASE;
                ELSE NULL;
                END CASE;  
                ELSE NULL;
                END CASE;                      
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al actualizar registro [pIngresoId] - '||pIngresoId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END ActualizarRegistro; 
         pResultado := 'Se ha actualizado el registro. [Id:'||pIngresoId||']';                              
     END CASE;
 EXCEPTION
 WHEN eSalidaConError THEN
      pResultado := pResultado;
      pMsgError  := vFirma||pMsgError;
 END PR_U_INGRESO;
 
 FUNCTION FN_VAL_EXISTE_INGRESO (pConsulta      IN HOSPITALARIO.OBJ_INGRESO_EGRESO,
                                 pTipoIngEgr    IN VARCHAR2,
                                 pPgn           IN BOOLEAN,
                                 pCantRegistros OUT NUMBER,
                                 pFuente        OUT NUMBER) RETURN BOOLEAN AS
                                 
 vContador SIMPLE_INTEGER := 0;
 vExiste BOOLEAN := FALSE;
 BEGIN
 CASE
 WHEN pConsulta.FecInicio IS NULL AND pConsulta.FecFin IS NULL THEN
  dbms_output.put_line ('Entra a validación fechas nulas');
  dbms_output.put_line ('pConsulta.IngresoId: '||pConsulta.IngresoId);
   CASE
   WHEN NVL(pTipoIngEgr,'X') = kEGRESO THEN   --- FSEQUEIRA
      CASE
      WHEN NVL(pConsulta.IngresoId,0) > 0 THEN
           BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS
             WHERE INGRESO_ID          = pConsulta.IngresoId AND
                   FECHA_FIN_INGRESO IS NOT NULL AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
           pFuente := 100;        
           END;
      WHEN (NVL(pConsulta.PerNominalId,0) > 0 AND
            NVL(pConsulta.ExpedienteId,0) > 0)  THEN
            BEGIN
             SELECT COUNT (1)
               INTO vContador
               FROM HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS A
               JOIN CATALOGOS.SBC_MST_PERSONAS_NOMINAL B
                 ON B.PER_NOMINAL_ID = A.PER_NOMINAL_ID
              WHERE A.PER_NOMINAL_ID = pConsulta.PerNominalId  AND
                    B.EXPEDIENTE_ID = pConsulta.ExpedienteId AND 
                    A.FECHA_FIN_INGRESO IS NOT NULL AND
                    A.ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
            pFuente := 101;
            END;  
      WHEN NVL(pConsulta.PerNominalId,0) > 0 THEN
           BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS    
             WHERE PER_NOMINAL_ID = pConsulta.PerNominalId  AND
                   FECHA_FIN_INGRESO IS NOT NULL AND
                   ESTADO_REGISTRO_ID != vGLOBAL_ESTADO_ELIMINADO;
           pFuente := 102;
           END;  
      WHEN NVL(pConsulta.ExpedienteId,0) > 0 THEN
           BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_INGRESOS_EGRESOS A
              JOIN CATALOGOS.SBC_MST_PERSONAS_NOMINAL B
                ON B.PER_NOMINAL_ID = A.PER_NOMINAL_ID         
             WHERE B.EXPEDIENTE_ID = pConsulta.ExpedienteId AND 
                   A.FECHA_FIN_INGRESO IS NOT NULL AND
