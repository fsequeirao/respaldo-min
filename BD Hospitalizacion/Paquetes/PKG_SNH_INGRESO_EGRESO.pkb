                              pResultado            OUT VARCHAR2,
                              pMsgError             OUT VARCHAR2) IS
 vFirma    VARCHAR2(200):= kPAQUETE_FIRMA||'PR_I_DIAGNOSTICOS => ';      
 vTipoDxId HOSPITALARIO.SNH_MST_DIAGNOSTICOS.TIPO_DIAGNOSTICO%TYPE;                      
 BEGIN
      vTipoDxId := HOSPITALARIO.PKG_SNH_UTILITARIOS.FN_OBT_CATALOGO_ID ('TDIAGNOSTICOING', pTipDxCod );
      pDxId := HOSPITALARIO.SEQ_SNH_DIAGNOSTICO_ID.NEXTVAL;
      INSERT INTO SNH_MST_DIAGNOSTICOS (DIAGNOSTICO_ID,
                                        ADMISION_SERVICIO_ID,
                                        MPERS_SALUD_ID,
                                        CIE_ID,
                                        USR_CODIFICA_CIE,
                                        TIPO_DIAGNOSTICO,
                                        DIAGNOSTICO,
                                      --  FECHA_SOLUCION,
                                        ESTADO_REGISTRO,
                                        USUARIO_REGISTRO)
           VALUES (pDxId,
                   pAdminServId,
                   pMpersSaludId,
                   pCieId,
                   pMperSaludCieCodifica,
                   vTipoDxId,
                   pDiagnostico,
                 --  pFechaSoluc,
                   vGLOBAL_ESTADO_ACTIVO,
                   pUsuario);
                   
 EXCEPTION 
 WHEN OTHERS THEN
      pResultado := vFirma||'Se ha generado un error inesperado al tratar de crear el diagnostico';
      pMsgError  := pResultado||' - '||SQLERRM;
 END PR_I_DIAGNOSTICOS;
 
 PROCEDURE PR_U_DIAGNOSTICOS (pDxId                 IN HOSPITALARIO.SNH_MST_DIAGNOSTICOS.DIAGNOSTICO_ID%TYPE,    
                              pAdminServId          IN HOSPITALARIO.SNH_MST_DIAGNOSTICOS.ADMISION_SERVICIO_ID%TYPE,
                              pMpersSaludId         IN HOSPITALARIO.SNH_MST_DIAGNOSTICOS.MPERS_SALUD_ID%TYPE,
                              pCieId                IN HOSPITALARIO.SNH_MST_DIAGNOSTICOS.CIE_ID%TYPE,
                              pMperSaludCieCodifica IN HOSPITALARIO.SNH_MST_DIAGNOSTICOS.USR_CODIFICA_CIE%TYPE,
                              pTipDxCod             IN CATALOGOS.SBC_CAT_CATALOGOS.CODIGO%TYPE, 
                              pDiagnostico          IN HOSPITALARIO.SNH_MST_DIAGNOSTICOS.DIAGNOSTICO%TYPE,
                              pEstadoRegistroId     IN HOSPITALARIO.SNH_MST_DIAGNOSTICOS.ESTADO_REGISTRO%TYPE,
                              pUsuario              IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE,  
                              pResultado            OUT VARCHAR2,
                              pMsgError             OUT VARCHAR2) IS

 vFirma    MAXVARCHAR2 := kPAQUETE_FIRMA||'PR_U_DIAGNOSTICOS => ';
 vTipoDxId HOSPITALARIO.SNH_MST_DIAGNOSTICOS.TIPO_DIAGNOSTICO%TYPE;
 BEGIN
     dbms_output.put_line ('Dentro de update dx');
     dbms_output.put_line ('pDxId: '||pDxId);
     dbms_output.put_line ('pAdminServId: '||pAdminServId);
     dbms_output.put_line ('pMpersSaludId: '||pMpersSaludId);
     dbms_output.put_line ('pMperSaludCieCodifica: '||pMperSaludCieCodifica);
     dbms_output.put_line ('pTipDxCod: '||pTipDxCod); 
     dbms_output.put_line ('pDiagnostico: '||pDiagnostico);
     dbms_output.put_line ('pEstadoRegistroId: '||pEstadoRegistroId);    
     CASE
     WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ELIMINADO THEN
         <<EliminaRegistro>>
          BEGIN
             UPDATE HOSPITALARIO.SNH_MST_DIAGNOSTICOS
                SET ESTADO_REGISTRO      = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,
                    USUARIO_ELIMINA      = pUsuario,
                    FECHA_ELIMINA        = CURRENT_TIMESTAMP
              WHERE DIAGNOSTICO_ID = pDxId AND
                    ESTADO_REGISTRO != vGLOBAL_ESTADO_ELIMINADO;
          EXCEPTION
             WHEN OTHERS THEN
                  pResultado := 'Error no controlado al eliminar registro [Id] - '||pDxId;
                  pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                  RETURN;                
          END EliminaRegistro;
          pResultado := 'Se ha eliminado el registro. [Id:'||pDxId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_PASIVO THEN
         <<PasivaRegistro>>       
         BEGIN
             UPDATE HOSPITALARIO.SNH_MST_DIAGNOSTICOS
                SET ESTADO_REGISTRO      = pEstadoRegistroId,
                    USUARIO_MODIFICACION = pUsuario,  
                    USUARIO_PASIVA       = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                           ELSE USUARIO_PASIVA
                                           END,    
                    FECHA_PASIVA         = CASE
                                           WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                           WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                           ELSE FECHA_PASIVA
                                           END
             WHERE DIAGNOSTICO_ID = pDxId AND
                   ESTADO_REGISTRO != vGLOBAL_ESTADO_ELIMINADO;     
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al pasivar registro [Id] - '||pDxId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END PasivaRegistro;
         pResultado := 'Se ha pasivado el registro. [Id:'||pDxId||']'; 
      WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN
         <<ActivarRegistro>>
         BEGIN
            UPDATE HOSPITALARIO.SNH_MST_DIAGNOSTICOS
               SET ESTADO_REGISTRO       = pEstadoRegistroId, 
                   USUARIO_MODIFICACION = pUsuario,    
                   USUARIO_PASIVA       = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN USUARIO_PASIVA IS NULL THEN pUsuario
                                          ELSE USUARIO_PASIVA
                                          END,    
                   FECHA_PASIVA         = CASE
                                          WHEN pEstadoRegistroId = vGLOBAL_ESTADO_ACTIVO THEN NULL
                                          WHEN FECHA_PASIVA IS NULL THEN CURRENT_TIMESTAMP
                                          ELSE FECHA_PASIVA
                                          END
             WHERE DIAGNOSTICO_ID = pDxId AND
                   ESTADO_REGISTRO != vGLOBAL_ESTADO_ELIMINADO;  
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al activar registro [Id] - '||pDxId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END ActivarRegistro; 
         pResultado := 'Se ha activado el registro. [Id:'||pDxId||']';                        
     ELSE 
         <<ActualizarRegistro>>
         BEGIN
--           CASE
--           WHEN NVL(pIsLast,0) = kES_PRINCIPAL THEN 
--               CASE        
--               WHEN FN_EXISTE_ADMSERV_CAMAS (pAdminServId, pCfgUsalServCamaId) = TRUE THEN
--                    PR_U_ISLAST_ADMSERV_CAMAS (pAdminServId       => pAdminServId,
--                                               pCfgUsalServCamaId => pCfgUsalServCamaId, 
--                                               pUsuario           => pUsuario,
--                                               pResultado         => pResultado,
--                                               pMsgError          => pMsgError);
--                    CASE
--                    WHEN pMsgError IS NOT NULL THEN
--                         pResultado := pResultado;
--                         pMsgError  := pMsgError;
--                         RAISE eSalidaConError;
--                    ELSE NULL;
--                    END CASE;                             
--               ELSE NULL;
--               END CASE;  
--           ELSE NULL;
--           END CASE;  
            vTipoDxId := HOSPITALARIO.PKG_SNH_UTILITARIOS.FN_OBT_CATALOGO_ID ('TDIAGNOSTICOING', pTipDxCod );     
            dbms_output.put_line ('vTipoDxId: '||vTipoDxId);
            dbms_output.put_line ('pDxId: '||pDxId);
            UPDATE HOSPITALARIO.SNH_MST_DIAGNOSTICOS
               SET MPERS_SALUD_ID       = pMpersSaludId,
                   ADMISION_SERVICIO_ID = pAdminServId,      
                   CIE_ID               = pCieId,  
                   USR_CODIFICA_CIE     = pMperSaludCieCodifica, 
                   TIPO_DIAGNOSTICO     = vTipoDxId,
                   DIAGNOSTICO          = pDiagnostico,
                   USUARIO_MODIFICACION = pUsuario          
             WHERE DIAGNOSTICO_ID = pDxId AND
                   ESTADO_REGISTRO != vGLOBAL_ESTADO_ELIMINADO;  
         EXCEPTION
            WHEN OTHERS THEN
                 pResultado := 'Error no controlado al activar registro [Id] - '||pDxId;
                 pMsgError  := vFirma||pResultado||' - '||sqlerrm;
                 RETURN;  
         END ActualizarRegistro; 
         pResultado := 'Se ha actualizado el registro. [Id:'||pDxId||']';                              
     END CASE;
 EXCEPTION
 WHEN eSalidaConError THEN
      pResultado := pResultado;   
      pMsgError  := vFirma||pMsgError;
 WHEN OTHERS THEN
      pResultado := 'Error no controlado';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM;
 END PR_U_DIAGNOSTICOS;
 
 FUNCTION FN_VAL_EXISTE_DIAGNOSTICO (pConsulta      IN HOSPITALARIO.OBJ_DIAGNOSTICOS,
                                     pPgn           IN BOOLEAN, 
                                     pCantRegistros OUT NUMBER,  
                                     pFuente        OUT NUMBER) RETURN BOOLEAN AS
 vRetorna BOOLEAN := FALSE;
 vContador SIMPLE_INTEGER := 0; 
 BEGIN
   CASE
   WHEN NVL(pConsulta.IngresoId,0) > 0 AND
        NVL(pConsulta.AdminServId,0) > 0 THEN 
        BEGIN
            SELECT COUNT (1)
            INTO vContador
            FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG REL
            JOIN HOSPITALARIO.SNH_MST_DIAGNOSTICOS DX
              ON DX.DIAGNOSTICO_ID = REL.DIAGNOSTICO_ID AND
                 DX.ADMISION_SERVICIO_ID = pConsulta.AdminServId
            WHERE REL.INGRESO_ID = pConsulta.IngresoId;        
        END; 
        pFuente := 1;
   WHEN NVL(pConsulta.TrasladoId,0) > 0 AND
        NVL(pConsulta.AdminServId,0) > 0 THEN 
        NULL;
        pFuente := 2;   
   WHEN NVL(pConsulta.mperSaludId,0) > 0 AND
        NVL(pConsulta.AdminServId,0) > 0 THEN
        BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_DIAGNOSTICOS
             WHERE ADMISION_SERVICIO_ID = pConsulta.AdminServId AND
                   MPERS_SALUD_ID = pConsulta.mperSaludId;
        END; 
        pFuente := 3;
   WHEN NVL(pConsulta.mperSaludId,0) > 0 AND
        NVL(pConsulta.IngresoId,0) > 0 THEN
        BEGIN
            SELECT COUNT (1)
            INTO vContador
            FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG REL
            JOIN HOSPITALARIO.SNH_MST_DIAGNOSTICOS DX
              ON DX.DIAGNOSTICO_ID = REL.DIAGNOSTICO_ID AND
                 DX.MPERS_SALUD_ID = pConsulta.mperSaludId
           WHERE REL.INGRESO_ID = pConsulta.IngresoId;        
        END; 
        pFuente := 4;
   WHEN NVL(pConsulta.TipoDxIngEg,0) > 0 AND
        NVL(pConsulta.IngresoId,0) > 0 THEN
        BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG A
             WHERE A.INGRESO_ID = pConsulta.IngresoId AND
                   A.TIPO_DX_ING_EG_ID = pConsulta.TipoDxIngEg ;        
        END; 
        pFuente := 5;
   WHEN NVL(pConsulta.TipoDxIngEg,0) > 0 AND
        NVL(pConsulta.AdminServId,0) > 0 THEN 
        BEGIN
            SELECT COUNT (1)
            INTO vContador
            FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG REL
            JOIN HOSPITALARIO.SNH_MST_DIAGNOSTICOS DX
              ON DX.DIAGNOSTICO_ID = REL.DIAGNOSTICO_ID AND
                 DX.ADMISION_SERVICIO_ID = pConsulta.AdminServId
           WHERE REL.TIPO_DX_ING_EG_ID = pConsulta.TipoDxIngEg;        
        END; 
        pFuente := 6;
   WHEN NVL(pConsulta.TipoDxId,0) > 0 AND
        NVL(pConsulta.IngresoId,0) > 0 THEN
        BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG A
              JOIN HOSPITALARIO.SNH_MST_DIAGNOSTICOS DX
              ON DX.DIAGNOSTICO_ID = A.DIAGNOSTICO_ID AND
                 DX.TIPO_DIAGNOSTICO = pConsulta.TipoDxId 
           WHERE A.INGRESO_ID = pConsulta.IngresoId;        
        END; 
        pFuente := 13;
   WHEN NVL(pConsulta.TipoDxId,0) > 0 AND
        NVL(pConsulta.AdminServId,0) > 0 THEN 
        BEGIN
            SELECT COUNT (1)
            INTO vContador
            FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG REL
            JOIN HOSPITALARIO.SNH_MST_DIAGNOSTICOS DX
              ON DX.DIAGNOSTICO_ID = REL.DIAGNOSTICO_ID AND
                 DX.ADMISION_SERVICIO_ID = pConsulta.AdminServId AND
                 DX.TIPO_DIAGNOSTICO = pConsulta.TipoDxId;        
        END; 
        pFuente := 14;        
   WHEN NVL(pConsulta.diagnosticoId,0) > 0 THEN
        BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_DIAGNOSTICOS
             WHERE DIAGNOSTICO_ID = pConsulta.diagnosticoId;
        END; 
        pFuente := 7;
   WHEN NVL(pConsulta.RelDxIngEgId,0) > 0 THEN     
        BEGIN
            SELECT COUNT (1)
            INTO vContador
            FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG REL
           WHERE REL_DX_ING_EG_ID = pConsulta.RelDxIngEgId;
        END;
        pFuente := 8;
   WHEN NVL(pConsulta.IngresoId,0) > 0 THEN     
        BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_REL_DIAGNOSTICOS_ING_EG A
             WHERE A.INGRESO_ID = pConsulta.IngresoId;        
        END;     
        pFuente := 9;
   WHEN NVL(pConsulta.TrasladoId,0) > 0 THEN     
        NULL; 
        pFuente := 10;
   WHEN NVL(pConsulta.AdminServId,0) > 0 THEN     
        BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_DIAGNOSTICOS
             WHERE ADMISION_SERVICIO_ID = pConsulta.AdminServId;
        END;       
        pFuente := 11; 
   WHEN NVL(pConsulta.mperSaludId,0) > 0 THEN     
        BEGIN
            SELECT COUNT (1)
              INTO vContador
              FROM HOSPITALARIO.SNH_MST_DIAGNOSTICOS
             WHERE MPERS_SALUD_ID = pConsulta.mperSaludId;
        END;
        pFuente := 12;
   ELSE NULL;
   END CASE;
   CASE
   WHEN vContador > 0 THEN
        vRetorna := TRUE;
   ELSE NULL;
   END CASE;
   pCantRegistros := vContador;
   RETURN vRetorna;   
 EXCEPTION
 WHEN OTHERS THEN
      RETURN vRetorna;   
 END FN_VAL_EXISTE_DIAGNOSTICO;
 
