 FUNCTION OBT_EXISTE_ADMIN_SRV_CAMA_ID (pAdmisionServicioId IN HOSPITALARIO.SNH_MST_ADMISION_SERVICIOS.ADMISION_SERVICIO_ID%TYPE) RETURN BOOLEAN AS
 vContador SIMPLE_INTEGER := 0;
 vExiste   BOOLEAN := FALSE;
 BEGIN
 SELECT COUNT (1)
   INTO vContador
   FROM HOSPITALARIO.SNH_REL_ADMSRV_CAMAS
  WHERE ADMISION_SERVICIO_ID = pAdmisionServicioId AND
        IS_LAST = 1;
  CASE
  WHEN vContador > 0 THEN
       vExiste := TRUE;
  ELSE NULL;
  END CASE;
 RETURN vExiste;
 EXCEPTION
 WHEN OTHERS THEN
      RETURN vExiste; 
 END OBT_EXISTE_ADMIN_SRV_CAMA_ID;

 FUNCTION OBT_ADMISION_SRV_CAMA_ID (pAdmisionServicioId IN HOSPITALARIO.SNH_MST_ADMISION_SERVICIOS.ADMISION_SERVICIO_ID%TYPE) RETURN NUMBER AS
 vId HOSPITALARIO.SNH_REL_ADMSRV_CAMAS.ADMISION_SRV_CAMA_ID%TYPE;
 BEGIN
 SELECT ADMISION_SRV_CAMA_ID
   INTO vId
   FROM HOSPITALARIO.SNH_REL_ADMSRV_CAMAS
  WHERE ADMISION_SERVICIO_ID = pAdmisionServicioId AND
        IS_LAST = 1;
 RETURN vId;
 EXCEPTION
 WHEN OTHERS THEN
      RETURN vId; 
 END OBT_ADMISION_SRV_CAMA_ID; 
  
