--            PRETTY) AS notas
--            FROM DUAL;
     RETURN  vRegistro;
 END FN_OBT_MST_NOTAS_PAG; 
 
 FUNCTION FN_OBT_DATOS_MST_NOTAS (pConsulta IN HOSPITALARIO.OBJ_MST_NOTAS,
                                  pPgn      IN BOOLEAN,
                                  pFuente   IN NUMBER) RETURN var_refcursor AS
 vRegistro var_refcursor;                                 
 BEGIN
     dbms_output.put_Line ('FN_OBT_DATOS_MST_NOTAS');
     CASE
     WHEN pFuente = 1 THEN
          vRegistro := FN_OBT_MST_NOTAS_DET_ID (pConsulta.detNotaId, pConsulta.persaludEvoNotaId);
          
     WHEN pFuente = 2 THEN
          vRegistro := FN_OBT_MST_NOTAS_ID (pConsulta.detNotaId);

     WHEN pFuente = 3 THEN
          vRegistro := FN_OBT_DET_NOTAS_ID (pConsulta.persaludEvoNotaId);

     ELSE 
          vRegistro := FN_OBT_MST_NOTAS_PAG;
     END CASE; 
     RETURN vRegistro;

 END FN_OBT_DATOS_MST_NOTAS; 
 
 PROCEDURE PR_C_MST_NOTAS (pConsulta        IN OBJ_MST_NOTAS,
                           pPgn             IN NUMBER,
                           pPgnAct          IN NUMBER default 1, 
                           pPgnTmn          IN NUMBER default 100,
                           pDatosPaginacion OUT var_refcursor,
                           pRegistro        OUT var_refcursor,    
                           pResultado       OUT VARCHAR2,         
                           pMsgError        OUT VARCHAR2) IS
                           
 vFirma         MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_C_MST_NOTAS => ';
 vCantRegistros SIMPLE_INTEGER := 0;
 vPaginacion    HOSPITALARIO.PKG_CATALOGOS_UTIL.assoc_array_str_datos; 
 vFuente        SIMPLE_INTEGER := 0;
 vPgn           BOOLEAN := TRUE;
 BEGIN
     CASE
     WHEN (FN_VAL_EXISTE_MST_NOTAS (pConsulta, vPgn, 
                                    vCantRegistros, vFuente)) = TRUE THEN 
                  DBMS_OUTPUT.PUT_LINE ('Existe mst notas: '||pMsgError);   
                   HOSPITALARIO.PKG_CATALOGOS_UTIL.PR_INDC_PAGINACION_PERSONA(PREGISTROS  => vCantRegistros, 
                                                                              pPgnAct     => pPgnAct, 
                                                                              pPgnTmn     => pPgnTmn, 
                                                                              pPaginacion => vPaginacion, 
                                                                              pMsg        => pMsgError);
                 DBMS_OUTPUT.PUT_LINE ('sale de paginación: '||pMsgError);                                                          
                 CASE 
                 WHEN pMsgError IS NOT NULL THEN 
                      pResultado := pMsgError;
                      pMsgError  := pMsgError;
                      RAISE eSalidaConError;
                 ELSE 
                      pDatosPaginacion := FN_OBT_DATOS_PAGINACION (pDatosPaginacion =>  vPaginacion);
                 END CASE;            
                 PR_I_TABLA_TEMP_MST_NOTAS (pConsulta       => pConsulta,
                                            pPgnAct         => pPgnAct,        
                                            pPgnTmn         => pPgnTmn,       
                                            pTipoPaginacion => vFuente,
                                            pResultado      => pResultado,     
                                            pMsgError       => pMsgError); 
                                 dbms_output.put_line ('Error saliendo de I tabla temporal PR_I_TABLA_TEMP_MST_NOTAS: '||pMsgError);              
                                 CASE
                                 WHEN pMsgError IS NOT NULL THEN 
                                      RAISE eSalidaConError;
                                 ELSE NULL;
                                 END CASE;
           pRegistro := FN_OBT_DATOS_MST_NOTAS(pConsulta, vPgn, vFuente);
     ELSE
           pResultado := 'No se encontraron registros con los parámetros enviados';
           RAISE eRegistroNoExiste;
     END CASE;
 EXCEPTION
 WHEN eSalidaConError THEN
      pResultado := pResultado;
      pMsgError  := vFirma||pResultado;  
 WHEN eRegistroNoExiste THEN
      pResultado := pResultado;
      pMsgError  := vFirma||pResultado;
 WHEN OTHERS THEN
      pResultado := 'Error no controlado al querer obtener información de notas.';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM;
 END PR_C_MST_NOTAS; 

 PROCEDURE PR_I_JSON_DATA (pIdJson    OUT HOSPITALARIO.SNH_JSON_DATA.ID%TYPE,
                           pJson      IN VARCHAR2,
                           pResultado OUT VARCHAR2,           
                           pMsgError  OUT VARCHAR2) IS
 vFirma MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_I_JSON_DATA => ';                          
 BEGIN
   INSERT INTO HOSPITALARIO.SNH_JSON_DATA (info)
                            VALUES (pJson)
                            RETURNING ID INTO pIdJson;
 EXCEPTION
 WHEN OTHERS THEN
      pResultado := 'Error no controlado al insertar el parámetro json';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM;     
 END PR_I_JSON_DATA;

 PROCEDURE PR_C_JSON_DATA_NOTAS (pIdJson    IN HOSPITALARIO.SNH_JSON_DATA.ID%TYPE,
                                 pRegistro  OUT refMstNotas,  --var_refcursor,
                                 pResultado OUT VARCHAR2,     
                                 pMsgError  OUT VARCHAR2) IS
 --vRegistro var_refcursor;
 vFirma MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_C_JSON_DATA_NOTAS => ';
 BEGIN
    DBMS_OUTPUT.PUT_LINE ('Dentro del begin de c_json: '||pIdJson);
    OPEN pRegistro FOR
         SELECT A.INFO.id, 
                A.INFO.admisionServicio.id, 
                A.INFO.fecha,
                A.INFO.hora, 
                A.INFO.personalSalud.id, 
                A.INFO.tipoNota.id,
                A.INFO.tipoNota.codigo
         FROM HOSPITALARIO.SNH_JSON_DATA a
        WHERE ID = pIdJson;
 EXCEPTION
 WHEN OTHERS THEN
      pResultado := 'Error no controlado al insertar el parámetro json';
      pMsgError  := vFirma||pResultado||' - '||SQLERRM;         
 END PR_C_JSON_DATA_NOTAS;
 
 PROCEDURE PR_CRUD_MST_NOTAS (pDetNotaId           IN OUT HOSPITALARIO.SNH_MST_NOTAS.DET_NOTA_ID%TYPE,
                              pAdmServId           IN HOSPITALARIO.SNH_MST_NOTAS.ADMISION_SERVICIO_ID%TYPE,
                              pFecNota             IN HOSPITALARIO.SNH_MST_NOTAS.FECHA_NOTA%TYPE,
                              pHrNota              IN HOSPITALARIO.SNH_MST_NOTAS.HORA_NOTA%TYPE,
                              pTipNota             IN HOSPITALARIO.SNH_MST_NOTAS.TIPO_NOTA_ID%TYPE,
                              pMpersSaludPrincipal IN HOSPITALARIO.SNH_MST_NOTAS.MPERS_SALUD_ID%TYPE,
                              pPerSaludEvoNotaId   IN OUT HOSPITALARIO.SNH_DET_NOTAS.PERSALUD_EVO_NOTA_ID%TYPE,
                              pTipNotaDetId        IN HOSPITALARIO.SNH_DET_NOTAS.TIPO_DET_NOTA_ID%TYPE,
                              pCometario           IN HOSPITALARIO.SNH_DET_NOTAS.COMENTARIOS%TYPE,
                              pUsuario             IN SEGURIDAD.SCS_MST_USUARIOS.USERNAME%TYPE, 
                              pAccionEstado        IN VARCHAR2,                                  
                              pFecInicio           IN DATE,
                              pFecFin              IN DATE, 
                              pJson                IN VARCHAR2,
                              pTipoAccion          IN VARCHAR2,
                              pPgn                 IN NUMBER,
                              pPgnAct              IN NUMBER default 1, 
                              pPgnTmn              IN NUMBER default 100,
                              pDatosPaginacion     OUT var_refcursor,
                              pRegistro            OUT var_refcursor,                           
                              pResultado           OUT VARCHAR2,                                
                              pMsgError            OUT VARCHAR2) IS
 vFirma             MAXVARCHAR2 := 'HOSPITALARIO.PKG_SNH_INGRESO_EGRESO.PR_CRUD_MST_NOTAS => ';
 vResultado         MAXVARCHAR2;
 vEstadoRegistroId  CATALOGOS.SBC_CAT_CATALOGOS.CATALOGO_ID%TYPE;  
 vIdJson            HOSPITALARIO.SNH_JSON_DATA.ID%TYPE;
 
 vRegMstNotas       refMstNotas;
 vVarRegMstNotas    mstNotas;

 vRegDetNotas       refDetNotas;
 vVarRegDetNotas    detNotas; 
 vExisteDetalle     SIMPLE_INTEGER := 0;
 
 vIdTipoNota       HOSPITALARIO.SNH_MST_NOTAS.TIPO_NOTA_ID%TYPE;
 BEGIN
      CASE
      WHEN pTipoAccion IS NULL THEN 
           pResultado := 'El párametro pTipoAccion no puede venir NULL';
           pMsgError  := pResultado;
           RAISE eParametroNull;
      ELSE NULL;
      END CASE;
      
      CASE
      WHEN pTipoAccion = kINSERT THEN
           CASE
           WHEN pUsuario IS NULL THEN
                pResultado := 'El usuario no puede venir nulo.';
                pMsgError  := pResultado;
                RAISE eParametroNull;
           ELSE 
                CASE -- validamos que el usuario se valido, con mst_usuarios
                WHEN (PKG_SNH_UTILITARIOS.FN_VALIDAR_USUARIO (pUsuario)) = FALSE THEN
                      pResultado := 'Usuario no valido';
                      pMsgError  := pResultado;
                      RAISE eRegistroNoExiste;
                ELSE NULL;
                END CASE;
           END CASE;
           dbms_output.put_line ('Sale de validar usuario');
           CASE
           WHEN pJson IS NOT NULL THEN
                dbms_output.put_line ('Trae parámetro json');
                PR_I_JSON_DATA_ENTRADA (pIdJson    => vIdJson,
                                        pJson      => pJson,
                                        pResultado => pResultado,           
                                        pMsgError  => pMsgError);
                   vIdJson := null;
                
                    CASE
                    WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                         RAISE eSalidaConError;
                    ELSE NULL;
                    END CASE;
                                         
                PR_I_JSON_DATA (pIdJson    => vIdJson,
                                pJson      => pJson,
                                pResultado => pResultado,           
                                pMsgError  => pMsgError); 
                    dbms_output.put_line ('vIdJson: '||vIdJson); 
                    dbms_output.put_line ('Saliendo de json pMsgError: '||pMsgError);             
                    CASE
                    WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                         RAISE eSalidaConError;
                    ELSE 
                        PR_C_JSON_DATA_NOTAS (pIdJson    => vIdJson,
                                              pRegistro  => vRegMstNotas,  --pRegistro,
                                              pResultado => pResultado,
                                              pMsgError  => pMsgError);
                                 dbms_output.put_line ('Saliendo de PR_C_JSON_DATA_NOTAS pMsgError: '||pMsgError);                 
                                CASE
                                WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                                     RAISE eSalidaConError;
                                ELSE 
                                     dbms_output.put_line ('antes de asignar variables de cursor');
                                     FETCH vRegMstNotas INTO vVarRegMstNotas;
                                     CLOSE vRegMstNotas;
                                     dbms_output.put_line ('despues de asignar variables de cursor');
                                END CASE;                        
                    END CASE;                                 
           ELSE NULL;
           END CASE;
           dbms_output.put_line ('vVarRegMstNotas.admisionServicioId: '||vVarRegMstNotas.admisionServicioId);
           dbms_output.put_line ('vVarRegMstNotas.fecha: '||vVarRegMstNotas.fecha);
           dbms_output.put_line ('vVarRegMstNotas.hora: '||vVarRegMstNotas.hora);
           dbms_output.put_line ('vVarRegMstNotas.tipoNotaId: '||vVarRegMstNotas.tipoNotaId);
           dbms_output.put_line ('vVarRegMstNotas.tipoNotaId: '||vVarRegMstNotas.codigo);
           dbms_output.put_line ('vVarRegMstNotas.mPersSaludId: '||vVarRegMstNotas.mPersSaludId);
           vIdTipoNota := HOSPITALARIO.PKG_SNH_UTILITARIOS.FN_OBT_TIPO_NOTA (vVarRegMstNotas.codigo);
           PR_I_MST_NOTAS (pDetNotaId           => pDetNotaId,                   
                           pAdmServId           => NVL(pAdmServId,vVarRegMstNotas.admisionServicioId),            --  pAdmServId,           
                           pFecNota             => NVL(pFecNota, TO_DATE(vVarRegMstNotas.fecha,'RRRR-MM-DD')),    --  pFecNota,             
                           pHrNota              => NVL(pHrNota,vVarRegMstNotas.hora),                             --  pHrNota,              
                           pTipNota             => NVL(pTipNota,vIdTipoNota),                                     -- vVarRegMstNotas.tipoNotaId),                     --  pTipNota,             
                           pMpersSaludPrincipal => NVL(pMpersSaludPrincipal,vVarRegMstNotas.mPersSaludId),        --  pMpersSaludPrincipal, 
                           pUsuario             => pUsuario,             
                           pResultado           => pResultado,           
                           pMsgError            => pMsgError);            
                    CASE
                    WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                         RAISE eSalidaConError;
                    WHEN NVL(pDetNotaId,0) > 0 THEN
                        PR_C_JSON_DATA_DETNOTAS (pIdJson    => vIdJson,
                                                 pRegistro  => vRegDetNotas,
                                                 pResultado => pResultado,
                                                 pMsgError  => pMsgError);
                                 dbms_output.put_line ('Saliendo de PR_C_JSON_DATA_DETNOTAS pMsgError: '||pMsgError);                 
                                CASE
                                WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                                     RAISE eSalidaConError;
                                ELSE NULL;
                                END CASE;                     
                          FETCH vRegDetNotas INTO vVarRegDetNotas;
                          vExisteDetalle := vRegDetNotas%ROWCOUNT;
                          CLOSE vRegDetNotas; 
                          dbms_output.put_line ('existe detalle: '||vExisteDetalle);
                         PR_D_JSON_TABLA (pId => vIdJson);
                         CASE
                         WHEN vExisteDetalle > 0 THEN
                         dbms_output.put_line ('llama a crud detalle');
                         PR_CRUD_DET_NOTAS (pPerSaludEvoNotaId => pPerSaludEvoNotaId, 
                                            pDetNotaId         => pDetNotaId,         
                                            pTipNotaDetId      => pTipNotaDetId,      
                                            pComentario        => pCometario,        
                                            pUsuario           => pUsuario,           
                                            pAccionEstado      => pAccionEstado,      
                                            pFecInicio         => pFecInicio,         
                                            pFecFin            => pFecFin,            
                                            pJson              => pJson,                                    
                                            pTipoAccion        => pTipoAccion,        
                                            pPgn               => pPgn,               
                                            pPgnAct            => pPgnAct,            
                                            pPgnTmn            => pPgnTmn,            
                                            pDatosPaginacion   => pDatosPaginacion,   
                                            pRegistro          => pRegistro,          
                                            pResultado         => pResultado,        
                                            pMsgError          => pMsgError); 
                             CASE
                             WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                                 RAISE eSalidaConError;
                             ELSE NULL;
                             END CASE;                                                     
                         ELSE NULL;
                         END CASE;
                         
                            DBMS_OUTPUT.PUT_LINE ('Antes de entrar al get de insert Id: '||pDetNotaId);
                          PR_C_MST_NOTAS (pConsulta            => HOSPITALARIO.OBJ_MST_NOTAS (pDetNotaId,
                                                                                              pAdmServId,
                                                                                              pFecNota,
                                                                                              pMpersSaludPrincipal,
                                                                                              null,
                                                                                              pTipNota,
                                                                                              null,
                                                                                              null,
                                                                                              null
                                                                                              ),
                                          pPgn                 => pPgn,                
                                          pPgnAct              => pPgnAct,             
                                          pPgnTmn              => pPgnTmn,             
                                          pDatosPaginacion     => pDatosPaginacion,    
                                          pRegistro            => pRegistro,           
                                          pResultado           => pResultado,          
                                          pMsgError            => pMsgError);           
                           DBMS_OUTPUT.PUT_LINE ('Sale de pro C notas despues de I. : '||pMsgError);                                    
                          CASE
                          WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                               RAISE eSalidaConError;
                          ELSE vResultado := 'Se crean exitosamente registro de notas de enfermería y/ evolución [Id]: '||pDetNotaId||', devolviendo el JSon de este';
                          END CASE;
                    ELSE NULL;  -- vResultado := 'Se crean exitosamente registro de notas de enfermería y/ evolución [Id]: '||pDetNotaId||', devolviendo el JSon de este';
                    END CASE;             
           
      WHEN pTipoAccion = kUPDATE THEN
           CASE
           WHEN pUsuario IS NULL THEN
                pResultado := 'El usuario no puede venir nulo.';
                pMsgError  := pResultado;
                RAISE eParametroNull;
           ELSE 
                CASE -- validamos que el usuario se valido, con mst_usuarios
                WHEN (PKG_SNH_UTILITARIOS.FN_VALIDAR_USUARIO (pUsuario)) = FALSE THEN
                      pResultado := 'Usuario no valido';
                      pMsgError  := pResultado;
                      RAISE eRegistroNoExiste;
                ELSE NULL;
                END CASE;
           END CASE;
---
          dbms_output.put_line ('Sale de validar usuario');
           CASE
           WHEN pJson IS NOT NULL THEN
                dbms_output.put_line ('Trae parámetro json');
                PR_I_JSON_DATA (pIdJson    => vIdJson,
                                pJson      => pJson,
                                pResultado => pResultado,           
                                pMsgError  => pMsgError); 
                    dbms_output.put_line ('vIdJson: '||vIdJson); 
                    dbms_output.put_line ('Saliendo de json pMsgError: '||pMsgError);             
                    CASE
                    WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                         RAISE eSalidaConError;
                    ELSE 
                        PR_C_JSON_DATA_NOTAS (pIdJson    => vIdJson,
                                              pRegistro  => vRegMstNotas,  --pRegistro,
                                              pResultado => pResultado,
                                              pMsgError  => pMsgError);
                                 dbms_output.put_line ('Saliendo de PR_C_JSON_DATA_NOTAS pMsgError: '||pMsgError);                 
                                CASE
                                WHEN pMsgError IS NOT NULL AND LENGTH (TRIM (pMsgError)) > 0 THEN
                                     RAISE eSalidaConError;
                                ELSE 
                                     dbms_output.put_line ('antes de asignar variables de cursor');
                                     FETCH vRegMstNotas INTO vVarRegMstNotas;
                                     CLOSE vRegMstNotas;
                                     dbms_output.put_line ('despues de asignar variables de cursor');
                                END CASE;                        
                    END CASE;                                 
           ELSE NULL;
           END CASE;
---                 
           CASE
           WHEN pAccionEstado = 0 THEN
                vEstadoRegistroId := vGLOBAL_ESTADO_ACTIVO;
           WHEN pAccionEstado = 1 THEN
                vEstadoRegistroId := vGLOBAL_ESTADO_PASIVO;
           ELSE NULL;
           END CASE;
           DBMS_OUTPUT.PUT_LINE ('1 - pDetNotaId: '||pDetNotaId);
           vIdTipoNota := HOSPITALARIO.PKG_SNH_UTILITARIOS.FN_OBT_TIPO_NOTA (vVarRegMstNotas.codigo);
